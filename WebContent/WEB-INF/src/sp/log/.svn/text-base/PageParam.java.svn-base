package sp.log;

import org.apache.struts.action.ActionForm;

public class PageParam extends ActionForm{

    private String nowPage;
    private String totalCount;
    private String rowPerPage; 
 
    private String v_scCODE;
    private String scFILDE;
    private String scORDER_NUM;
    
    
	public String getNowPage() {
		return nowPage;
	}
	public void setNowPage(String nowPage) {
		this.nowPage = nowPage;
	}
	public String getRowPerPage() {
		return rowPerPage;
	}
	public void setRowPerPage(String rowPerPage) {
		this.rowPerPage = rowPerPage;
	}
	public String getScFILDE() {
		return scFILDE;
	}
	public void setScFILDE(String scFILDE) {
		this.scFILDE = scFILDE;
	}
	public String getScORDER_NUM() {
		return scORDER_NUM;
	}
	public void setScORDER_NUM(String scORDER_NUM) {
		this.scORDER_NUM = scORDER_NUM;
	}
	public String getTotalCount() {
		return totalCount;
	}
	public void setTotalCount(String totalCount) {
		this.totalCount = totalCount;
	}
	public String getV_scCODE() {
		return v_scCODE;
	}
	public void setV_scCODE(String v_sccode) {
		v_scCODE = v_sccode;
	}
    
}
