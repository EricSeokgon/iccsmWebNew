package kjf.util;

public class KJFArea {
    
    /**
     * 시도 코드를 인수로 받아서 시도명을 리턴한다.
     *
     * @param SD_Code   시도 코드.
     * @return String   시도명
     */
    public static  String getSido_Name(String sido_code) throws  Exception {
        
        if(sido_code.equals("gwgw")) return "강원";
        else if (sido_code.equals("gggg")) return "경기";
        else if(sido_code.equals("gngn")) return "경남";
        else if(sido_code.equals("gbgb")) return "경북";
        else if(sido_code.equals("gjgj")) return "광주";
        else if(sido_code.equals("dgdg")) return "대구";
        else if(sido_code.equals("djdj")) return "대전";
        else if(sido_code.equals("bsbs")) return "부산";
        else if(sido_code.equals("susu")) return "서울";
        else if(sido_code.equals("sjsj")) return "세종";
        else if(sido_code.equals("usus")) return "울산";
        else if(sido_code.equals("icic")) return "인천";
        else if(sido_code.equals("jnjn")) return "전남";
        else if(sido_code.equals("jbjb")) return "전북";
        else if(sido_code.equals("jjjj")) return "제주";
        else if(sido_code.equals("cncn")) return "충남";
        else if(sido_code.equals("cbcb")) return "충북";
        
     
        else return "기타";
    }

}
