function fnForm_Loaded(obj)
{
	fnTablesBinding();
	
	ZTransferX0.DownloadServer = global.g_ext_oz_url + "ozviewer";
	ZTransferX0.StartInstall();
}

function fnForm_ReLoad(obj)
{
	this.Reload();
}


