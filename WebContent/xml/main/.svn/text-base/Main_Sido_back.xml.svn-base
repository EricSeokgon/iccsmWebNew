function fnDday()
{
    var dt1 = dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(dsPT_R_BASIC_CHANGE_HISTORY.row, "BAS_STA_DT2");
    var name = dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(dsPT_R_BASIC_CHANGE_HISTORY.row, "name")+", ";    
    var dt2 =Today();
    var cnt = 1;
    for ( var i =0; i <= dsPT_R_BASIC_CHANGE_HISTORY.count; i++){
	  dt1 = dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(i, "BAS_STA_DT2");
	  dt2 = Today();
	 

	 if (dt1 <= dt2 )
	 {

		 cnt += 1;
		 
		if ((cnt % 4) == 1 ){				
		   name += dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(i, "name")+", \n\n";			
		} else {
		  name += dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(i+1, "name")+", ";
		}

	 }	
    }

    var msg=" 의 신고마감일이 ";
    var msg2="일 남았습니다.";
    var msg3="\n\n 처리하시겠습니까?";
    var dt1 = dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(dsPT_R_BASIC_CHANGE_HISTORY.row, "BAS_STA_DT2");    
    var dt2 =Today();
    var sumday = ParseDateTime(dt1)-ParseDateTime(dt2);
    var dt3 = "## 오늘 날짜 "+substr(today(),0,4)+"년 "+substr(today(),4,2)+"월 "+substr(today(),6,2)+"일 입니다. ##"+"\n\n";   
    
   
        if(dt1 <> null){    
        if(ParseDateTime(dt1)-ParseDateTime(dt2)<8){
	if(!Confirm(dt3+name+msg+sumday+msg2+msg3)) return;
        
        var sWorkFlag = global.gv_MODIFY;
		//var sArgs = "_sWorkFlag=" + quote(sWorkFlag) + " _sKEY1=" + quote(sKEY1);
		global.MainMenu.Visible = true;
		gfnCreateNewWindow("StdNearSt", "cm::StdNearSt.xml");
	}
	}
	else	{
	return;
	}
}
