	<Script><![CDATA[
/*
1. 파일명 : RegReceiptMgr.xml
2. 작성일 : 2009-02-06
3. 작성자 : 김 현 기
4. 기  능 : 공사업체 등록신청 접수현황조회
5. 변경이력
  가. 2009-02-06 최초작성 (김현기)
*/

#include "UserLib::CommonJs.js"

//////////////////////////////////////////////////////////////////
// 전역변수
//////////////////////////////////////////////////////////////////
var	_bDialog = false;
var base_code = " WRT_ID="+global.g_User_ID+" SIDO_CODE="+global.g_Sd_cd+" SIGUNGU_CODE="+global.g_Sgg_cd;
//////////////////////////////////////////////////////////////////
// Form Load 이벤트
//////////////////////////////////////////////////////////////////
function fnForm_Loaded(obj,nRow)
{
	fnTablesBinding();
	
	ZTransferX0.DownloadServer = global.g_ext_oz_url + "ozviewer";
	ZTransferX0.StartInstall();

}

function fnForm_ReLoad(obj)
{
	this.Reload();
	
	
}

function fnForm_Activate(obj)
{
	global.g_activeFrm = this.Id;
	
	
}

//////////////////////////////////////////////////////////////////
// 공통기능
//////////////////////////////////////////////////////////////////
/**
 1. 함수명 : fnInitButtonBar
 2. 기  능 : 화면상단 버튼상태 설정
 3. Params 
    없음
 4. Return
    없음
*/
function fnInitButtonBar()
{
	if (global.gv_WorkFlag == global.gv_INQUIRY)
	{
		fnAllControlState(true, this);
	
		divButtonBar.btnInsert.enable = true;
		divButtonBar.btnModify.enable = false;
		divButtonBar.btnRemove.enable = true;
		
		divButtonBar.btnSave.enable = false;
		divButtonBar.btnReport.enable = false;
		divButtonBar.btnExcel.enable = true;
		divButtonBar.btnCancel.enable = false;
		
	}
	else if (global.gv_WorkFlag == global.gv_INSERT)
	{
		divButtonBar.btnInsert.enable = false;
		divButtonBar.btnModify.enable = false;
		divButtonBar.btnRemove.enable = false;
		
		divButtonBar.btnSave.enable = fnSetMenuSaveButtonAuth(2, true);
		divButtonBar.btnReport.enable = false;
		divButtonBar.btnExcel.enable = false;
		
		fnAllControlState(true, this);
	}
	else
	{
		divButtonBar.btnInsert.enable = false;
		divButtonBar.btnModify.enable = false;
		divButtonBar.btnRemove.enable = false;
		
		divButtonBar.btnSave.enable = fnSetMenuSaveButtonAuth(2, true);
		divButtonBar.btnReport.enable = false;
		divButtonBar.btnExcel.enable = false;
	}
}

/**
 1. 함수명 : fnTablesBinding
 2. 기  능 : 검색조건에 대한 검색
 3. Params 
    없음
 4. Return
    없음
*/
function fnTablesBinding()
{

	var sArgs = "scWORKTYPE=SIDO";
	    sArgs += base_code;
	
	var sTid = "SEARCH";
	var sUrl = "ICCS::main/Main_mAction.do?cmd=CenterBoardSIDOMgr";
	var sInDs = "";
	var sOutDs = "ds_CENTER_BOARD1=output ds_CENTER_BOARD2=output2 ds_CENTER_BOARD3=output3 ds_V_PT_MAIN_SIDO1=output4 ds_V_PT_MAIN_SIDO2=output5 dsPT_R_BASIC_CHANGE_HISTORY=output6";
	var sCallBack = "fn_CallBack";
	
	Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
	
}



function fn_CallBack(Svcid, sErrCode, sErrMsg) 
{
