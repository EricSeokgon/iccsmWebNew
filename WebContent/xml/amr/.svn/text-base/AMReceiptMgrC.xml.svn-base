	var sOutDs = "dsPT_M_MGR=output dsMASTER=output1 dsPT_R_COMPANY_MASTER=output2 dsMGR=output3";
	var sCallBack = "fn_CallBack";
	
	Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
       
}

