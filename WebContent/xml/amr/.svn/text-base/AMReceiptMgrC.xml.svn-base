function Div1_NOTE_RECV_YN_OnClick(obj,strValue)
{

if(Div1.NOTE_RECV_YN.value == "1"){
	Div1.DISPO_DT.Visible = true;
	Div1.BUSISUSP_END_DT.Visible =true;
	Div1.BUSISUSP_START_DT.Visible =true;
	Div1.Static6.Visible =true;
	Div1.Static7.Visible =true;
	Div1.Static8.Visible =true;
	Div1.Image4.Visible =true;
	Div1.Image5.Visible =true;
	}
	else{
	Div1.DISPO_DT.Visible =false;
	Div1.BUSISUSP_END_DT.Visible =false;
	Div1.BUSISUSP_START_DT.Visible =false;
	Div1.Static6.Visible =false;
	Div1.Static7.Visible =false;
	Div1.Static8.Visible =false;
	Div1.Image4.Visible =false;
	Div1.Image5.Visible =false;
	}
}

