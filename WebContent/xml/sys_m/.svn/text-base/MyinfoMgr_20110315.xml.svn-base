function fnToolBarButton_Click(obj)
{
	if (obj == divButtonBar.btnInsert)
	{
		fnInsert();
	}
	else if (obj == divButtonBar.btnModify)
	{
		fnModify();		
	}
	else if (obj == divButtonBar.btnRemove)
