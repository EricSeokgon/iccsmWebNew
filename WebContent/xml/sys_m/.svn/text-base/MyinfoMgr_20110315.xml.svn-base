function fn_CallBack(Svcid, sErrCode, sErrMsg) 
{
	if (sErrCode < 0) 	
	{
		alert(sErrCode + " : " + sErrMsg);
		if (svcid == "SEARCH")
		{
			alert('데이터 조회 중 오류가 발생하였습니다.');
			return;
		}
		
		if (scvid == "save")
		{
			alert('데이터 저장 중 오류가 발생하였습니다.');
			return;
		}
	}
	
	if (svcid == "save")
	{
		alert('저장되었습니다.');
		global.gv_WorkFlag = global.gv_INQUIRY;
		fnInitButtonBar();
		return;
	}
	
	if (svcid == "SEARCH")
	{
		var img_name = ds_PT_MI_USER.GetColumn(ds_PT_MI_USER.row,"OFFSEAL");
	
		if(img_name != "" && img_name != "null"){
			// 2009-09-29 수정
			var url = global.g_ext_iccs_url + "file/Offseal/"+global.g_Sgg_cd+"/"+global.g_User_ID+"/"+UrlEncode(img_name);
			
			this.DemoImage.ImageID = url;
		}
		
		
		// 2009-09-29 추가
		var img_name_per = ds_PT_MI_USER.GetColumn(ds_PT_MI_USER.row,"PHOTO");
		
		if(img_name_per != "" && img_name_per != "null"){
			var url_per = global.g_ext_iccs_url + "file/User/"+global.g_Sd_cd+"/"+global.g_User_ID+"/"+UrlEncode(img_name_per);			
			this.PersonImage.ImageID = url_per;
		}
	
		/*var target_server = ds_UB_ENDPOINT.GetColumn(ds_UB_ENDPOINT.row,"TARGET_SERVER");
		if (target_server !="true"){
		  this.Microsoft0.Visible = "false";
		  this.Static24.Visible = "false";
		  this.Image19.Visible = "false";
		}
		*/
		if (this.Combo1.text == ""){
			this.Combo1.index = 0;
		}

	}
	
}

