	<Script><![CDATA[/*
1. 파일명 : RegReceiptMgr.xml
2. 작성일 : 2009-02-26
3. 작성자 : 김 현 기
4. 기  능 : 공사업체 접수내역조회 다이얼로그
5. 변경이력
  가. 2009-02-26 최초작성 (김현기)
  나. 2009-04-10 Y.D Jung
*/

#include "UserLib::CommonJs.js"

//////////////////////////////////////////////////////////////////
// 전역변수
//////////////////////////////////////////////////////////////////
var	scTB_NM;
var scSEQ;
var scCOI_WRT_NUM;
var scMANA_NUM;

//////////////////////////////////////////////////////////////////
// Form Load 이벤트
//////////////////////////////////////////////////////////////////
function fnForm_Loaded(obj)
{
	fnTablesBinding();
}

function fnTablesBinding()
{
	var sArgs = "scTB_NM=" + quote(scTB_NM);
	sArgs += " scSEQ=" + quote(scSEQ);
	sArgs += " scCOI_WRT_NUM=" + quote(scCOI_WRT_NUM);
	sArgs += " scMANA_NUM=" + quote(scMANA_NUM);
	sArgs += " scMODE=DETAIL";
	
	var sTid = "SEARCH";
	var sUrl = "ICCS::sys_m/Sys_mAction.do?cmd=KICAIfRecv";
	var sInDs = "";
	var sOutDs = "dsPT_R_COMPANY_MASTER=output dsPT_TMP_REP_CHANGE=output2";
	var sCallBack = "fn_CallBack";
	
	Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
}

function fn_CallBack(Svcid, sErrCode, sErrMsg) 
{
	if (sErrCode < 0) 	
	{
		alert(sErrCode + " : " + sErrMsg);
		if (svcid == "SEARCH")
		{
			alert('데이터 조회 중 오류가 발생하였습니다.');
		}

		return;
	}
	
	if (Svcid == "SAVE")
	{
		alert('변경내용이 시스템에 적용되었습니다.');
		Close(true);
	}
}

function fnKey_Down(obj,objSenderObj,nChar,bShift,bControl,bAlt,nLLParam,nHLParam)
{

}
function btnRecv_OnClick(obj)
{
	dsPT_R_BASIC_CHANGE_HISTORY.AddRow();
	dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "STA_CHG_CLASS_CODE", "R00009");
	dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "SIDO_CODE", global.g_Sd_cd);
	dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "WRT_ID", global.g_User_ID);
	dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "INT_DT", Today());
	dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "TMP_WRT_NUM", dsPT_R_COMPANY_MASTER.GetColumn(0, "TMP_WRT_NUM"));
	dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "COI_WRT_NUM", dsPT_R_COMPANY_MASTER.GetColumn(0, "COI_WRT_NUM"));
	dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "MANA_NUM", dsPT_R_COMPANY_MASTER.GetColumn(0, "MANA_NUM"));
	
	dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "REP_NM_KOR", dsPT_TMP_REP_CHANGE.GetColumn(0, "REP_NM_KOR"));
	dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "REP_NM_HAN", dsPT_TMP_REP_CHANGE.GetColumn(0, "REP_NM_HAN"));
	dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "REP_SSN1", dsPT_TMP_REP_CHANGE.GetColumn(0, "REP_SSN1"));
	dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "REP_SSN2", dsPT_TMP_REP_CHANGE.GetColumn(0, "REP_SSN2"));
	dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "CHG_DT", dsPT_TMP_REP_CHANGE.GetColumn(0, "CHG_STA_DT"));
	
	dsPT_R_COMPANY_MASTER.SetColumn(0, "SIDO_CODE",  global.g_Sd_cd);
	dsPT_R_COMPANY_MASTER.SetColumn(0, "REP_NM_KOR", dsPT_TMP_REP_CHANGE.GetColumn(0, "REP_NM_KOR"));
	dsPT_R_COMPANY_MASTER.SetColumn(0, "REP_NM_HAN", dsPT_TMP_REP_CHANGE.GetColumn(0, "REP_NM_HAN"));
	dsPT_R_COMPANY_MASTER.SetColumn(0, "REP_SSN1", dsPT_TMP_REP_CHANGE.GetColumn(0, "REP_SSN1"));
	dsPT_R_COMPANY_MASTER.SetColumn(0, "REP_SSN2", dsPT_TMP_REP_CHANGE.GetColumn(0, "REP_SSN2"));
	
	var sArgs = "workMODE=ALL";
	var sTid = "SAVE";
	var sUrl = "ICCS::sys_m/Sys_mAction.do?cmd=KICAIfRecvCUD";
	var sInDs = "dsPT_R_COMPANY_MASTER=dsPT_R_COMPANY_MASTER:u dsPT_R_BASIC_CHANGE_HISTORY=dsPT_R_BASIC_CHANGE_HISTORY:u ";
	var sOutDs = "dsPT_R_COMPANY_MASTER=output1 dsPT_R_BASIC_CHANGE_HISTORY=output2";
	var sCallBack = "fn_CallBack";
	
	Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
}]]></Script>
