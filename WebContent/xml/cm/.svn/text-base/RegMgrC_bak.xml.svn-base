					dsPT_R_BASIC_CHANGE_HISTORY.GetColumn( dsPT_R_BASIC_CHANGE_HISTORY.row, "REP_SSN2");
		
		dsREFER_DUPLICATE.DeleteAll();
		var sArgs = "workMODE=REFER_DUPLICATE_CHECK scTMP_WRT_NUM="+ quote(_sKEY1);
		var sTid = "REFER_DUPLICATE_CHECK";
		var sUrl = "ICCS::cm/CmAction.do?cmd=RegMgrC";
		var sInDs = "dsPT_R_REFFER=dsPT_R_REFFER";
		var sOutDs = "dsREFER_DUPLICATE=output";
		var sCallBack = "fn_CallBack";
		
		Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
		http.Sync = false;
		
		if( dsREFER_DUPLICATE != null && dsREFER_DUPLICATE.rowcount > 0 ) {
			alert( dsREFER_DUPLICATE.GetColumn(0, "NM_KOR") + " 은(는) " + dsREFER_DUPLICATE.GetColumn(0, "NAME") + " 업체에 기술자로 재직 중입니다.\n" +
			 "\n대표자, 임원정보를 확인하시기 바랍니다.\n데이터를 저장하지 않았습니다.");
			return;
		}
		//-- 종료 :: 대표자가 타업체 재직기술자인지 체크
		
		//-- 시작 :: 기술자가 타업체 대표자인지 체크
		http.Sync = true;
		
		dsREFER_DUPLICATE.DeleteAll();
		var sArgs = "workMODE=ENGINEER_DUPLICATE_CHECK scTMP_WRT_NUM="+ quote(_sKEY1);
		var sTid = "ENGINEER_DUPLICATE_CHECK";
		var sUrl = "ICCS::cm/CmAction.do?cmd=RegMgrC";
		var sInDs = "dsPT_R_ENGINEER_CHANGE=dsPT_R_ENGINEER_CHANGE";
		var sOutDs = "dsREFER_DUPLICATE=output";
		var sCallBack = "fn_CallBack";
		
		Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
		http.Sync = false;
		
		if( dsREFER_DUPLICATE != null && dsREFER_DUPLICATE.rowcount > 0 ) {
			alert( dsREFER_DUPLICATE.GetColumn(0, "NM_KOR") + " 은(는) " + dsREFER_DUPLICATE.GetColumn(0, "NAME") + " 업체에 대표자로 재직 중입니다.\n" +
			 "\n기술자 정보를 확인하시기 바랍니다.\n데이터를 저장하지 않았습니다.");
			return;
		}
		//-- 종료 :: 기술자가 타업체 대표자인지 체크
	
	
		//var wrtDt = Today();
		var wrtDt = TMP_WRT_DT.Value;
		
		if( fnEmpty(wrtDt) ) {
			alert( "등록완료일자를 입력해주십시오." );
			return;
		}

		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COI_WRT_NUM")))
		{
			alert('공사업등록번호가 입력되지 않았습니다.');
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		
		// 2010.01.08. 개인사업자의 경우 납입자본금을 입력할 수 없다.
		/*
		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "PAY_CAP")))
		{
			alert('납입 자본금 정보가 입력되지 않았습니다.');
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		*/
		
		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "TUP_AOM")))
		{
			alert('자본금 예치 정보가 입력되지 않았습니다.');
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		
		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "REA_CAP")))
		{
			alert('실질 자본금 정보가 입력되지 않았습니다.');
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		
		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COMMANA_CLASS")))
		{
			alert('사업자 구분 정보가 입력되지 않았습니다.');
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		
		// 2010.01.08. 사업자 구분에 따른 임원정보 확인
		var commanaClass = dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COMMANA_CLASS");
		if( commanaClass == "1" ) {
			for(var nLoop = 0; nLoop < dsPT_R_REFFER.rowcount; nLoop++)
			{
				if( dsPT_R_REFFER.GetColumn(nLoop, "POS_CLASS" ) == "P4" ) {
					alert( "법인사업자의 경우 임원은 대표자로 추가 할 수 없습니다." );
					MOT_STE.Value = "STD002";
					fnCheckMotSte();
					return;
				}
			}
		}
		else if( commanaClass == "2" ) {
			if( dsPT_R_REFFER.rowcount > 1 ) {
				alert( "개인사업자의 경우 임원은 대표자 1명만 추가 할 수 있습니다." );
				MOT_STE.Value = "STD002";
				fnCheckMotSte();
				return;
			}
			
			for(var nLoop = 0; nLoop < dsPT_R_REFFER.rowcount; nLoop++)
			{
				if( dsPT_R_REFFER.GetColumn(nLoop, "POS_CLASS" ) != "P4" ) {
					alert( "개인사업자의 경우 임원은 대표자 1명만 추가 할 수 있습니다." );
					MOT_STE.Value = "STD002";
					fnCheckMotSte();
					return;
				}
			}
		}
		
		var nAmt = toNumber(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "PAY_CAP"));
		var nRAmt = toNumber(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "REA_CAP"));
		var nTAmt = toNumber(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "TUP_AOM"));
		
		if (dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COMMANA_CLASS") == "1" && nAmt < 150000)
		{
			var sMsg = "법인의 경우 납입 자본금이 150,000,000 이상 등록이 가능합니다\n\n";
				sMsg += "                등록을 완료할 수 없습니다.                ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		
		if (dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COMMANA_CLASS") == "1" && nRAmt < 150000)
		{
			var sMsg = "법인의 경우 실질 자본금이 150,000,000 이상 등록이 가능합니다\n\n";
				sMsg += "                등록을 완료할 수 없습니다.                ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		
		if (dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COMMANA_CLASS") == "2" && nRAmt < 200000)
		{
			var sMsg = "개인의 경우 실질 자본금이 200,000,000 이상 등록이 가능합니다\n\n";
				sMsg += "                등록을 완료할 수 없습니다.                ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		
		if (dsPT_R_ENGINEER_CHANGE.rowcount <= 2) // 3인이상
		{
			var sMsg = "정보통신 기술자 보유숫자가 부족합니다.\n\n";
				sMsg += "    등록을 완료할 수 없습니다.     ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		else
		{
			var sEngVal = "X";
			for (var nLoop = 0; nLoop < dsPT_R_ENGINEER_CHANGE.rowcount; nLoop++)
			{
				var sGrade = dsPT_R_ENGINEER_CHANGE.GetColumn(nLoop, "TMP_FIELD");
				
				if (trim(sGrade) == "특급" || trim(sGrade) == "중급" || trim(sGrade) == "고급")
				{
					sEngVal = "O";
					break;
				}
			}
			
			if(sEngVal == "X")
			{
				var sMsg = "중급이상 기술자 보유숫자가 부족합니다.\n\n";
					sMsg += "    등록을 완료할 수 없습니다.     ";
				alert(sMsg);
				MOT_STE.Value = "STD002";
				fnCheckMotSte();
				return;
			}
		}
		
		if (dsPT_R_REFUSAL_REASON.rowcount > 0)
		{
			var sMsg = "등록 결격 사유가 존재합니다.\n\n";
					sMsg += "   등록을 완료할 수 없습니다. ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		
		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COMPANY_DIAG_ISSUE_DT")))
		{
			var sMsg = "기업진단보고서 발급일자가가 입력되지 않았습니다.\n\n";
					sMsg += "   등록을 완료할 수 없습니다. ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;		
		}
		
		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COMPANY_DIAG_BAS_DT")))
		{
			var sMsg = "기업진단보고서 기준일자가가 입력되지 않았습니다.\n\n";
					sMsg += "   등록을 완료할 수 없습니다. ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;		
		}
		
		var sDay = dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COMPANY_DIAG_BAS_DT"); 
				
		// 2009-09-18 수정 방송통신위원회고시 제2009-16호 30일->45일변경
		if(sDay > AddDate(sDay, 45)) 		
		{  
			var sMsg = "기업진단보고서 기준일자가가 45일 이내에 발급되지 않았습니다.\n\n";
				sMsg += "                등록을 완료할 수 없습니다.                 ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;		
		}

		if(ToInteger(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "OFFICE_AREA")) <= 14)			
		{
			var sMsg = "사무실 면적이 부족합니다.\n\n";
					sMsg += "등록을 완료할 수 없습니다. ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;		
		}
		
		/*
		공사업 등록번호를 수동생성으로 변경되어 주석처리함
		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COI_WRT_NUM")))
		{
			fnGetCOI_WRT_NUM();
			if (dsCOI_WRT_NUM.rowcount == 0)
			{
				var sMessage = "공사업등록번호 생성에 실패하였습니다.\n\n";
				sMessage += "    관리자에게 문의해주십시오.";
				alert(sMessage);
				MOT_STE.Value = "STD002";
				return;
			}
		}
		*/
		
		//기술자 입사일자를 등록일자로 자동반영
		for(var nLoop = 0; nLoop < dsPT_R_ENGINEER_CHANGE.rowcount; nLoop++)
		{
			dsPT_R_ENGINEER_CHANGE.SetColumn(nLoop, "EMPL_YMD", wrtDt);
		}
		
		//임원 입사일자를 등록일자로 자동반영
		for(var nLoop = 0; nLoop < dsPT_R_REFFER.rowcount; nLoop++)
		{
			dsPT_R_REFFER.SetColumn(nLoop, "ENT_DT", wrtDt);
		}
		
		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "WRT_DT")))
		{
			dsPT_R_COMPANY_MASTER.SetColumn(0, "WRT_DT", wrtDt);
			dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "WRT_DT", wrtDt);
		}
		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "BAS_STA_DT")))
		{
			dsPT_R_COMPANY_MASTER.SetColumn(0, "BAS_STA_DT", wrtDt);
			dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "BAS_STA_DT", wrtDt);
		}
		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "CHG_DT")))
			dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "CHG_DT", wrtDt);
		
		// 2010.01.28. 세부변경(ABCD) 넣기
		// 최초 등록이기 때문에 모든 값이 등록되어 있다.
		dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "EXT_CHG_CLASS_CODE", "ABCD");
	}
	
	fnSetDetailDataset();

	if (MOT_STE.Value == "STD003")
	{
		// 등록완료시 협회송신 큐에 넣기
		dsPT_TMP_TRANS_KEY.DeleteAll();
		dsPT_TMP_TRANS_KEY.AddRow();
		dsPT_TMP_TRANS_KEY.SetColumn(dsPT_TMP_TRANS_KEY.row, "WORK_CLASS_CODE", 
					dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "STA_CHG_CLASS_CODE"));
		dsPT_TMP_TRANS_KEY.SetColumn(dsPT_TMP_TRANS_KEY.row, "KEY1", 
					dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "TMP_WRT_NUM"));
		dsPT_TMP_TRANS_KEY.SetColumn(dsPT_TMP_TRANS_KEY.row, "KEY2", 
					dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "CHGBRE_SEQ"));
		if(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "SND_STE") == "O")
			dsPT_TMP_TRANS_KEY.SetColumn(dsPT_TMP_TRANS_KEY.row, "CUD_FLAG", "U");
		else
			dsPT_TMP_TRANS_KEY.SetColumn(dsPT_TMP_TRANS_KEY.row, "CUD_FLAG", "C");
		dsPT_TMP_TRANS_KEY.SetColumn(dsPT_TMP_TRANS_KEY.row, "SIDO_CODE", global.g_Sd_cd);
		dsPT_TMP_TRANS_KEY.SetColumn(dsPT_TMP_TRANS_KEY.row, "INS_DT", Today());
		
		http.Sync = true;
		var sArgs = "";		
		var sTid = "KICAIfSendCUD";
		var sUrl = "ICCS::sys_m/Sys_mAction.do?cmd=KICAIfSendCUD";
		var sInDs = "dsPT_TMP_TRANS_KEY=dsPT_TMP_TRANS_KEY:u";
		var sOutDs = "dsPT_TMP_TRANS_KEY=output";
		var sCallBack = "fn_CallBack";
		
		Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
		http.Sync = false;
	}
	
	var sArgs = "workMODE=ALL";
	var sTid = "SAVE";
	var sUrl = "ICCS::cm/CmAction.do?cmd=RegMgrCCUD";
	var sInDs = "dsPT_R_COMPANY_MASTER=dsPT_R_COMPANY_MASTER:u dsPT_R_BASIC_CHANGE_HISTORY=dsPT_R_BASIC_CHANGE_HISTORY:u dsPT_R_ENGINEER_CHANGE=dsPT_R_ENGINEER_CHANGE:u dsPT_R_MOT_STE=dsPT_R_MOT_STE:u dsPT_R_REFFER=dsPT_R_REFFER:u dsPT_R_COM_DEFICIT=dsPT_R_COM_DEFICIT:u";
	
	var sOutDs = "dsPT_R_COMPANY_MASTER=output dsPT_R_BASIC_CHANGE_HISTORY=output2 dsPT_R_ENGINEER_CHANGE=output3 dsPT_R_MOT_STE=output4 dsPT_R_REFFER=output5";
	var sCallBack = "fn_CallBack";

	Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
	
}

