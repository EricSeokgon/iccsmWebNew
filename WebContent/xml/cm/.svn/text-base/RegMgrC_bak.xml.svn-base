function fnLocalGridHead_Click(obj,nCell,nX,nY,nPivotIndex)
{
	var objDs = obj.BindDataset;

	if (object(objDs).RowCount() == 0)
		return;
	 
	if (obj.GetCellProp("head", nCell, "edit") == "checkbox")
	{
		if(obj.GetCellProp("head", nCell, "text") == "1")
		{
			obj.SetCellProp("head", nCell, "text", "0");
			fnSetGridCheck(0, objDs, "DEFI_OUT");
		}
		else
		{
			obj.SetCellProp("head", nCell, "text", "1");
			fnSetGridCheck(1, objDs, "DEFI_OUT");
		}
		
		object(objDs).ApplyChange();
	}
	else
	{
		for(var nLoop = 0; nLoop < obj.GetColCount(); nLoop++)
		{
			if(nLoop == nCell) continue;
			
			obj.SetCellProp("head", nLoop, "text", replace(obj.GetCellProp("head", nLoop, "text"), "▼", ""));
			obj.SetCellProp("head", nLoop, "text", replace(obj.GetCellProp("head", nLoop, "text"), "▲", ""));
		}
		
		var sHeadName = obj.GetCellProp("head", nCell, "text");
		var sColName = obj.GetCellProp("body", nCell, "colid");
		
		if (right(sHeadName, 1) == "▼") 
		{
			object(objDs).Sort(sColName, true);
			obj.SetCellProp("head", nCell, "text", replace(sHeadName, "▼", "▲"));
		}
		else if (right(sHeadName, 1) == "▲") 
		{
			object(objDs).Sort(sColName, false);
			obj.SetCellProp("head", nCell, "text", replace(sHeadName, "▲", "▼"));
		}
		else
		{
			object(objDs).Sort(sColName, true);
			obj.SetCellProp("head", nCell, "text", sHeadName + "▼");
		}
	}
}

function fnControlChanged(obj, strPreText, strPostText)
{
	if (obj == tbMain.tpReper.SSN1 || obj == tbMain.tpEngeneer.ENGINEER_SSN1 || obj == tbMain.tbBUB.REP_SSN1)
	{
		if(Length(strPostText) == obj.MaxLength)
		{
			GetNextComponent(true).SetFocus();
			return;
		}
		
		return;
	}
}

function RegMgrC_OnDeactivate(obj)
{
	if (global.gv_WorkFlag == global.gv_INSERT || global.gv_WorkFlag == global.gv_MODIFY)
	{
		if (!dsPT_R_BASIC_CHANGE_HISTORY.GetUpdate())
		{
			if(!Confirm('입력중인 데이터가 저장되지 않았습니다.\n\n      저장하시겠습니까?')) return;
			http.sync = true;
			fnSave();
		}
	}
}

function fnGetCOI_WRT_NUM()
{
	http.sync = true;
	
	var sArgs = "CHECK=TMP_CODE scDOC_CLASS=COI_WRT_NUM scSD_CODE=" + quote(global.g_Sd_cd);
	var sTid = "GET_COI_WRT_NUM";
	var sUrl = "ICCS::cm/CmAction.do?cmd=RegWrtNumC";
	var sInDs = "";
	var sOutDs = "dsCOI_WRT_NUM=output";
	var sCallBack = "fn_CallBack";
	
	Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
	
	if (dsCOI_WRT_NUM.rowcount == 0)
	{
		var sMessage = "공사업등록번호 생성에 실패하였습니다.\n\n";
		sMessage += "    관리자에게 문의해주십시오.";
		alert(sMessage);
		return;
	}
}

function fnLocalControlState()
{
	if (dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(dsPT_R_BASIC_CHANGE_HISTORY.row, "DIAG_ORG_CODE") == "DIG003")
		fnControlState(false, tbMain.tpAmt, "PAY_CAP");
	else
		fnControlState(true, tbMain.tpAmt, "PAY_CAP");
		
	if (dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(dsPT_R_BASIC_CHANGE_HISTORY.row, "COMMANA_CLASS") != "1")
	{
		fnControlState(false, tbMain.tpAmt, "PAY_CAP");
		tbMain.tpAmt.PAY_CAP.ToolTipText = "법인이 아닌 경우 납입자본금을 입력하실 수 없습니다.";
	}
	else
	{
		fnControlState(true, tbMain.tpAmt, "PAY_CAP");
		tbMain.tpAmt.PAY_CAP.ToolTipText = null;
	}
	
	fnCheckMotSte();

}

