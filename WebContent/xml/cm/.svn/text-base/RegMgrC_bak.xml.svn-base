	<Script><![CDATA[/*
1. 파일명 : RegC.xml
2. 작성일 : 2009-02-26
3. 작성자 : 김 현 기
4. 기  능 : 공사업체 등록
5. 변경이력
  가. 2009-02-26 최초작성 (김현기)
*/

#include "UserLib::CommonJs.js"
//////////////////////////////////////////////////////////////////
// 전역변수
//////////////////////////////////////////////////////////////////
var	_sKEY1;
var	_sKEY2;
var _sWorkFlag;

//////////////////////////////////////////////////////////////////
// Form Load 이벤트
//////////////////////////////////////////////////////////////////
function fnForm_Loaded(obj)
{
	//alert(_sWorkFlag);
	global.gv_WorkFlag = _sWorkFlag;
	
	if(global.gv_WorkFlag == global.gv_INQUIRY)
	{
		fnInquiry();
	}
	else if(global.gv_WorkFlag == global.gv_INSERT)
	{
		global.gv_WorkFlag = global.gv_INSERT;
		fnInitButtonBar();
	}
	else if(global.gv_WorkFlag == global.gv_MODIFY)
	{
		fnModify();
	}
	
	fnInitReperGroup("INQUIRY");
	fnInitEngineerGroup("INQUIRY");
	fnInitMemoGroup("INQUIRY");
	fnInitCompanyGroup("INQUIRY");
	fnInitRefusalGroup("INQUIRY");
	
	divButtonBar.Image1.text = space(6) + "공사업체 신규등록";
	
	tbMain.MoveTab(1, 0);
	tbMain.MoveTab(5, 2);
	tbMain.TabIndex = 0;
}

function fnForm_Activate(obj)
{
	global.g_activeFrm = this.Id;
}

//////////////////////////////////////////////////////////////////
// 공통기능
//////////////////////////////////////////////////////////////////
/**
 1. 함수명 : fnInitButtonBar
 2. 기  능 : 화면상단 버튼상태 설정
 3. Params 
    없음
 4. Return
    없음
*/
function fnInitButtonBar()
{
	if (global.gv_WorkFlag == global.gv_INQUIRY)
	{
		fnAllControlState(false, this);
		
		divButtonBar.btnInsert.enable = false;
		divButtonBar.btnModify.enable = true;
		divButtonBar.btnRemove.enable = false;
		
		divButtonBar.btnSave.enable = fnSetMenuSaveButtonAuth(2, false);
		divButtonBar.btnCancel.enable = false;
		divButtonBar.btnReport.enable = true;
		divButtonBar.btnExcel.enable = false;
		divButtonBar.btnList.enable = true;
				
		btnGetREC.Enable = false;
		tbmain.tpAmt.PAY_CAP.Enable = false;		
		fnLocalControlState();
	}
	else if (global.gv_WorkFlag == global.gv_INSERT)
	{
		fnAllControlState(true, this);
		
		divButtonBar.btnInsert.enable = false;
		divButtonBar.btnModify.enable = false;
		divButtonBar.btnRemove.enable = false;
		
		divButtonBar.btnSave.enable = false;
		divButtonBar.btnCancel.enable = false;
		divButtonBar.btnReport.enable = false;
		divButtonBar.btnExcel.enable = false;
		divButtonBar.btnList.enable = true;
		
		tbmain.tpAmt.PAY_CAP.Enable = true;
		btnGetREC.Enable = true;
		fnLocalControlState();
	}
	else
	{
		fnAllControlState(true, this);
		
		divButtonBar.btnInsert.enable = false;
		divButtonBar.btnModify.enable = false;
		divButtonBar.btnRemove.enable = false;
		
		divButtonBar.btnSave.enable = fnSetMenuSaveButtonAuth(2, true);
		divButtonBar.btnCancel.enable = true;
		divButtonBar.btnReport.enable = true;
		divButtonBar.btnExcel.enable = false;
		divButtonBar.btnList.enable = true;
				
		fnControlState(false, this, "RECV_NUM,NAME");
		tbmain.tpAmt.PAY_CAP.Enable = true;
		btnGetREC.Enable = false;
		fnLocalControlState();
	}
}


/**
 1. 함수명 : fnTablesBinding
 2. 기  능 : 검색조건에 대한 검색
 3. Params 
    없음
 4. Return
    없음
*/
function fnTablesBinding()
{
	var sArgs = "workMODE=RegMgrC scTMP_WRT_NUM=" + quote(_sKEY1) + " scRECV_NUM=" + quote(_sKEY2);
	var sTid = "SEARCH";
	var sUrl = "ICCS::cm/CmAction.do?cmd=RegMgrC";
	var sInDs = "";

	var sOutDs  = "dsPT_R_COMPANY_MASTER=output dsPT_R_BASIC_CHANGE_HISTORY=output2 " ;
		sOutDs += "dsPT_R_REFFER=output8 dsPT_R_ENGINEER_CHANGE=output7 " ;
		sOutDs += "dsPT_R_REG_MEMO=output9 dsPT_R_REFUSAL_REASON=output10 ";
		sOutDs += "dsPT_R_SUBSIDIARY=output11 dsPT_R_COM_DEFICIT=output12 dsPT_R_MOT_STE=output13 ";
		
	var sCallBack = "fn_CallBack";
	
		
	Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
}

function fn_CallBack(Svcid, sErrCode, sErrMsg) 
{
	http.sync = false;
	
	if (sErrCode < 0) 	
	{
		if (svcid == "SEARCH")
		{
			alert('데이터 조회 중 오류가 발생하였습니다.');
			return;
		}
		if (svcid == "SAVE")
		{
			alert('데이터 저장 중 오류가 발생하였습니다.');
			return;
		}
	}
	
	// 협회 송신 큐에 넣기가 성공한 경우
	if (svcid == "KICAIfSendCUD")
	{
		dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "SND_STE", "O");
		dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "SND_DT", Today());
	}
	
	if (svcid == "GET_TMP_CODE")
	{
		_sKEY1 = dsPT_R_DOC_NUM.GetColumn(0, "DOC_NUM1") + 
					dsPT_R_DOC_NUM.GetColumn(0, "DOC_NUM2");
		
		fnSetMasterDetail("dsPT_R_REGIST_STATEMENT", "dsPT_R_COMPANY_MASTER");
		fnSetMasterDetail("dsPT_R_REGIST_STATEMENT", "dsPT_R_BASIC_CHANGE_HISTORY");
		
		// 2010.01.28. 접수일자 가져오기
		dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "REC_DT", dsPT_R_REGIST_STATEMENT.GetColumn(0, "RECV_DT"));
		
		dsPT_R_COMPANY_MASTER.SetColumn(0, "TMP_WRT_NUM", _sKEY1);

		dsPT_R_COMPANY_MASTER.SetColumn(0, "SIDO_CODE", global.g_Sd_cd);
		dsPT_R_COMPANY_MASTER.SetColumn(0, "WRT_ID", global.g_User_ID);
		dsPT_R_COMPANY_MASTER.SetColumn(0, "COMPANY_DIAG_CLASS_CODE", "P1");
		
		dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "TMP_WRT_NUM", _sKEY1);
		dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "RECV_NUM", _sKEY2);
		dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "MOT_STE", "STD002");
		dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "STA_CHG_CLASS_CODE", "R00001");
		dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "SND_STE", "X");
		dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "SIDO_CODE", global.g_Sd_cd);
		dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "WRT_ID", global.g_User_ID);
		dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "COMPANY_DIAG_CLASS_CODE", "P1");
		
		// 2010.01.27. INS_DT 는 SYSDATE 로 유지토록 변경
		//dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "INS_DT", Today());
		
		tbMain.tpAmt.CAPITAL_CHECK_YN.Index = 1;
		tbMain.tpAmt.DIAGNOSTIC_DIV.Index = 0;
		
		for(var nLoop = 0; nLoop < dsPT_R_SUBSIDIARY_COMPANY.rowcount; nLoop++)
		{
			dsPT_R_SUBSIDIARY.AddRow();
			dsPT_R_SUBSIDIARY.SetColumn(nLoop, "TMP_WRT_NUM", _sKEY1);
			dsPT_R_SUBSIDIARY.SetColumn(nLoop, "SEQ", dsPT_R_SUBSIDIARY_COMPANY.GetColumn(nLoop, "SEQ"));
			dsPT_R_SUBSIDIARY.SetColumn(nLoop, "SUB_WRT_NUM", dsPT_R_SUBSIDIARY_COMPANY.GetColumn(nLoop, "OTOB_COM_NUM"));
			dsPT_R_SUBSIDIARY.SetColumn(nLoop, "SUB_CODE", dsPT_R_SUBSIDIARY_COMPANY.GetColumn(nLoop, "OTOB_COM_CODE"));
			dsPT_R_SUBSIDIARY.SetColumn(nLoop, "SUB_NAME", dsPT_R_SUBSIDIARY_COMPANY.GetColumn(nLoop, "OTOB_COM_NAME"));
		}
		
		// 접수시 등록한 메모 가져오기
		for(var nLoop = 0; nLoop < dsPT_R_MEMO.rowcount; nLoop++)
		{
			dsPT_R_REG_MEMO.AddRow();
			dsPT_R_REG_MEMO.SetColumn(nLoop, "TMP_WRT_NUM", _sKEY1);
			dsPT_R_REG_MEMO.SetColumn(nLoop, "CHGBRE_SEQ", "1");
			dsPT_R_REG_MEMO.SetColumn(nLoop, "MEMO_SEQ", dsPT_R_MEMO.GetColumn(nLoop, "MEMO_SEQ"));
			dsPT_R_REG_MEMO.SetColumn(nLoop, "MEMO_CONT", dsPT_R_MEMO.GetColumn(nLoop, "MEMO_CONT"));
			dsPT_R_REG_MEMO.SetColumn(nLoop, "WRT_DT", dsPT_R_MEMO.GetColumn(nLoop, "WRT_DT"));
			dsPT_R_REG_MEMO.SetColumn(nLoop, "UPD_DT", dsPT_R_MEMO.GetColumn(nLoop, "UPD_DT"));
			dsPT_R_REG_MEMO.SetColumn(nLoop, "INS_DT", dsPT_R_MEMO.GetColumn(nLoop, "INS_DT"));
			
		}
		
		// 임원정보 대표이사 1개 넣기.
		dsPT_R_REFFER.AddRow();
		dsPT_R_REFFER.SetColumn(0, "SEQ", 1);
		dsPT_R_REFFER.SetColumn(0, "TMP_WRT_NUM", dsPT_R_COMPANY_MASTER.GetColumn(0, "TMP_WRT_NUM"));
		dsPT_R_REFFER.SetColumn(0, "SI_DO_CODE", dsPT_R_COMPANY_MASTER.GetColumn(0, "SIDO_CODE"));
		dsPT_R_REFFER.SetColumn(0, "POS_CLASS", "P1");
		dsPT_R_REFFER.SetColumn(0, "POS_NAME", "대표이사");
		dsPT_R_REFFER.SetColumn(0, "NM_KOR", dsPT_R_COMPANY_MASTER.GetColumn(0, "REP_NM_KOR"));
		dsPT_R_REFFER.SetColumn(0, "NM_HAN", dsPT_R_COMPANY_MASTER.GetColumn(0, "REP_NM_HAN"));
		dsPT_R_REFFER.SetColumn(0, "SSN1", dsPT_R_COMPANY_MASTER.GetColumn(0, "REP_SSN1"));
		dsPT_R_REFFER.SetColumn(0, "SSN2", dsPT_R_COMPANY_MASTER.GetColumn(0, "REP_SSN2"));
		
		
		var sArgs = "workMODE=ALL";
		var sTid = "INSERT_SAVE";
		var sUrl = "ICCS::cm/CmAction.do?cmd=RegMgrCCUD";
		var sInDs = "dsPT_R_COMPANY_MASTER=dsPT_R_COMPANY_MASTER:u dsPT_R_BASIC_CHANGE_HISTORY=dsPT_R_BASIC_CHANGE_HISTORY:u dsPT_R_SUBSIDIARY=dsPT_R_SUBSIDIARY:u dsPT_R_REG_MEMO=dsPT_R_REG_MEMO:u";
		var sOutDs = "dsPT_R_COMPANY_MASTER=output dsPT_R_BASIC_CHANGE_HISTORY=output2 dsPT_R_SUBSIDIARY=output4 ";
		var sCallBack = "fn_CallBack";

		Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);	
	}
		
	if (svcid == "GET_RECV_NUM")
	{
		if (dsPT_R_REGIST_STATEMENT.GetColumn(0, "DEFI_STE") != "O")
		{
			alert('선택하신 접수건은 흠결이 완료되지 않았습니다.');
			Close();
		}
		

		fnInsert();
	}
	
	if (svcid == "REFER_CHECK")
	{
		if (dsReferCheck.rowcount > 0)
		{
			if (fnEmpty(dsReferCheck.GetColumn(0, "RETIRE_DATE")))
			{
				alert('입력하신 조회 대상자는 타 공사업체에 재직중입니다.');
				return;
			}
			else
			{
				alert('조회 대상자는 등록이 가능한 사람입니다.');
			}
		}
	}


	if (svcid == "SAVE")
	{
		alert('저장되었습니다.');
		fnInquiry();
	}
	
	if (svcid == "INSERT_SAVE")
	{
		dsPT_R_COMPANY_MASTER.ApplyChange();
		dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "CHGBRE_SEQ", "1");
		dsPT_R_BASIC_CHANGE_HISTORY.ApplyChange();
		
		fnSetMasterDetail("dsPT_R_BASIC_CHANGE_HISTORY", "dsPT_R_MOT_STE");
		
		global.gv_WorkFlag = global.gv_MODIFY;
		fnInitButtonBar();
	}
	
		
	if (svcid == "REPER_REPORT")
	{
		fnReferReport();
	}
	if (svcid == "REPER_SAVE")
	{
		alert('저장되었습니다.');
		fnInitReperGroup("INQUIRY");
	}
	if (svcid == "REPER_DELETE")
	{
		alert('삭제되었습니다.');
		fnInitReperGroup("INQUIRY");
	}
	if (svcid == "ENGINEER_SAVE")
	{
		alert('저장되었습니다.');
		fnInitEngineerGroup("INQUIRY");
	}
	if (svcid == "ENGINEER_DELETE")
	{
		alert('삭제되었습니다.');
		fnInitEngineerGroup("INQUIRY");
	}
	
	if (svcid == "MEMO_SAVE")
	{
		alert('저장되었습니다.');
		fnInitMemoGroup("INQUIRY");
	}
	if (svcid == "MEMO_DELETE")
	{
		alert('삭제되었습니다.');
		fnInitMemoGroup("INQUIRY");
	}
	if (svcid == "REFUSAL_SAVE")
	{
		alert('저장되었습니다.');
		fnInitRefusalGroup("INQUIRY");
	}
	if (svcid == "REFUSAL_DELETE")
	{
		alert('삭제되었습니다.');
		fnInitRefusalGroup("INQUIRY");
	}
	if (svcid == "COMPANY_SAVE")
	{
		alert('저장되었습니다.');
		fnInitCompanyGroup("INQUIRY");
	}
	if (svcid == "COMPANY_DELETE")
	{
		alert('삭제되었습니다.');
		fnInitCompanyGroup("INQUIRY");
	}
	
	if (svcid == "GET_COI_WRT_NUM")
	{
		var sCOI_WRT_NUM = ToString(dsCOI_WRT_NUM.GetColumn(0, "DOC_NUM1")) + 
								ToString(dsCOI_WRT_NUM.GetColumn(0, "DOC_NUM2"));
		dsPT_R_COMPANY_MASTER.SetColumn(0, "COI_WRT_NUM", sCOI_WRT_NUM);
		dsPT_R_BASIC_CHANGE_HISTORY.SetColumn(0, "COI_WRT_NUM", sCOI_WRT_NUM);
	}
	
	if (svcid == "SEARCH")
	{
		fnLocalControlState();
	}
}

