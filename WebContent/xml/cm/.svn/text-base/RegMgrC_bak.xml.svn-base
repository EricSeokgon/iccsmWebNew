/**
 * 등록상태 변경시 등록완료일자 입력 활성/비활성 처리
**/
function MOT_STE_OnChanged(obj,strCode,strText,nOldIndex,nNewIndex)
{
	if( MOT_STE.Value = "STD003" ) {
		MOT_STE.Enable = true;
		TMP_WRT_DT.Enable = true;
		if( fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "CHG_DT")) ) {
			TMP_WRT_DT.Value = Today();
		}
	}
	else {
		MOT_STE.Enable = true;
		TMP_WRT_DT.Enable = true;
		TMP_WRT_DT.Value = "";
	}
}

function fnCheckMotSte() {
	if( MOT_STE.Value = "STD003" ) {
		MOT_STE.Enable = true;
		TMP_WRT_DT.Enable = true;
		if( !fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "CHG_DT")) )
			TMP_WRT_DT.Value = dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "CHG_DT");
		else
			TMP_WRT_DT.Value = "";
	}
	else {
		MOT_STE.Enable = true;
		TMP_WRT_DT.Enable = true;
		TMP_WRT_DT.Value = "";
	}
}

