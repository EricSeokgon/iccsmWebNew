function tbMain_tpEngeneer_ENGINEER_NM_OnKeyDown(obj,nChar,bShift,bCtrl, bAlt,LLParam,HLParam)
{

	if(nChar == 13)
  {

//이곳에서 조회 내용을 코딩하세요 
		var sSSN1 = tbMain.tpEngeneer.ENGINEER_SSN1.Text;
	var sSSN2 = tbMain.tpEngeneer.ENGINEER_SSN2.Text;
	var sENGINEER_NM = tbMain.tpEngeneer.ENGINEER_NM.text;
	var sCORP = dsPT_R_COMPANY_MASTER.GetColumn(0, "COI_WRT_NUM");
	
	if ((fnEmpty(sSSN1) || fnEmpty(sSSN2)) && fnEmpty(sENGINEER_NM))
	{
		alert('기술자 주민등록번호 또는 기술자 이름을 입력하세요.');
		return;
	}
	
	var sArgs = "_sOldCode=" + quote(sSSN1 + sSSN2) + " _sNm=" + quote(sENGINEER_NM) + " _sCorp=" + quote(sCORP);
	
	var bRet = fnCreateDialog("st::EngListMgrA.xml", sArgs);
	
	if (bRet != null)
	{
	
		dsPT_R_ENGINEER_CHANGE_TEMP.SetColumn(dsPT_R_ENGINEER_CHANGE_TEMP.row, "ENGINEER_NM", trim(bRet[0]));
		dsPT_R_ENGINEER_CHANGE_TEMP.SetColumn(dsPT_R_ENGINEER_CHANGE_TEMP.row, "ENGINEER_SSN", trim(bRet[1]) + trim(bRet[2]));
		dsPT_R_ENGINEER_CHANGE_TEMP.SetColumn(dsPT_R_ENGINEER_CHANGE_TEMP.row, "ENGINEER_SSN1", trim(bRet[1]));
		dsPT_R_ENGINEER_CHANGE_TEMP.SetColumn(dsPT_R_ENGINEER_CHANGE_TEMP.row, "ENGINEER_SSN2", trim(bRet[2]));
		dsPT_R_ENGINEER_CHANGE_TEMP.SetColumn(dsPT_R_ENGINEER_CHANGE_TEMP.row, "CARE_BOOK_ISSUE_NUM", bRet[3]);
		dsPT_R_ENGINEER_CHANGE_TEMP.SetColumn(dsPT_R_ENGINEER_CHANGE_TEMP.row, "TMP_FIELD", bRet[4]);
		dsPT_R_ENGINEER_CHANGE_TEMP.SetColumn(dsPT_R_ENGINEER_CHANGE_TEMP.row, "CARE_BOOK_VAL_START_DT", bRet[5]);
		dsPT_R_ENGINEER_CHANGE_TEMP.SetColumn(dsPT_R_ENGINEER_CHANGE_TEMP.row, "CARE_BOOK_VAL_END_DT", bRet[6]);
	}
	

//기술자 중복체크
var rtn;
	
for(i=0;i<dsPT_R_ENGINEER_CHANGE_TEMP.count;i++)
{
rtn = dsPT_R_ENGINEER_CHANGE_TEMP.SearchRow(" ENGINEER_SSN1 = '" + dsPT_R_ENGINEER_CHANGE_TEMP.GetColumn(i,"ENGINEER_SSN1")+ "' &&  ENGINEER_SSN2 = '" + dsPT_R_ENGINEER_CHANGE_TEMP.GetColumn(i,"ENGINEER_SSN2")+ "'",i+1,dsPT_R_ENGINEER_CHANGE_TEMP.count);
 if(rtn != -1 ) //같은 값이 있을때
 {
 alert("중복 입력 하였습니다");
 dsPT_R_ENGINEER_CHANGE_TEMP.DeleteRow(dsPT_R_ENGINEER_CHANGE_TEMP.row);
 sDtWorkFlag = global.gv_INQUIRY;
 fnInitEngineerGroup(sDtWorkFlag);
 return false;
 break;
 }
}
EngListSav();	
  }
 
}

