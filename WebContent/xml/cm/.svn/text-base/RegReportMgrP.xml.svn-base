/**
 1. 함수명 : fnReport
 2. 기  능 : 출력버튼 이벤트 핸들러
 3. Params 
    없음
 4. Return
    없음
*/
function fnReport()
{

	var count = dsPT_R_BASIC_CHANGE_HISTORY.rowcount;
	
	var strWhereInParam = "0";
	
	var strRowPos = cbIndex.Value;
	
	var strShowBorder = optShowBorder.Value;
	
	var strViwerName = "";
	
	// 앞장
	if( BOOKDIV.Value == "1" ) {
		strViwerName = "RegCertificate1";
	}
	// 뒷장
	
	else {
		strViwerName = "RegCertificate2";
		
    if (count == 0 ){ 
    var sViewer = "_sViewer=" + strViwerName;
	var sReportname =  " _sReportname=connection.reportname=/BusinessRegist/" + strViwerName + ".ozr";
	var sOdiNames = " _sOdiNames=odi.odinames=Q)RegCertificate";
	var	spCount = " _spCount=odi.Q)RegCertificate.pcount=4";
	var sParam = " _sParam=odi.Q)RegCertificate.args1=TMP_WRT_NUM=" + _sKEY1 + "||";
	sParam += "odi.Q)RegCertificate.args2=SEQ_IN_PARAM=" + strWhereInParam + "||";
	sParam += "odi.Q)RegCertificate.args3=SHOW_BORDER=" + strShowBorder + "||";
	sParam += "odi.Q)RegCertificate.args4=ROW_POS=" + strRowPos;
	var sRet = fnCreateDialogResize("bas::frmReport.xml", sViewer + sReportname + sOdiNames + spCount + sParam);

		return;
		}
		
		var con=dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(dsPT_R_BASIC_CHANGE_HISTORY.row,"CHECKER");
		if(con==0){ 
			var sViewer = "_sViewer=" + strViwerName;
			var sReportname =  " _sReportname=connection.reportname=/BusinessRegist/" + strViwerName + ".ozr";
			var sOdiNames = " _sOdiNames=odi.odinames=Q)RegCertificate";
			var	spCount = " _spCount=odi.Q)RegCertificate.pcount=4";
			var sParam = " _sParam=odi.Q)RegCertificate.args1=TMP_WRT_NUM=" + _sKEY1 + "||";
			sParam += "odi.Q)RegCertificate.args2=SEQ_IN_PARAM=" + strWhereInParam + "||";
			sParam += "odi.Q)RegCertificate.args3=SHOW_BORDER=" + strShowBorder + "||";
			sParam += "odi.Q)RegCertificate.args4=ROW_POS=" + strRowPos;
			var sRet = fnCreateDialogResize("bas::frmReport.xml", sViewer + sReportname + sOdiNames + spCount + sParam);

		    return;
		}
		
		/*if (dsPT_R_BASIC_CHANGE_HISTORY.FindRow("CHECKER", "1") < 0){
			alert('인쇄할 데이터를 선택하세요.');
		
			return;
		}*/ 
	  
	  
		var chkCnt = 0;
		for (var nLoop = 0; nLoop < dsPT_R_BASIC_CHANGE_HISTORY.rowcount; nLoop++) {
			if (dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(nLoop, "CHECKER") == "1" ) {
				strWHEREInParam += dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(nLoop, "CHGBRE_SEQ") + ",";
				chkCnt++;
			}			
		}
		
		// 리포트의 행은 27개이다. 체크한 항목이 페이지를 벗어나면 안된다. :P
		if( strRowPos + chkCnt > 28 ) {
			alert( "한 페이지 최대 출력 갯수(27개)를 초과하였습니다." );
			return;
		}
		
		strWhereInParam = substr( strWhereInParam, 0, length( strWhereInParam ) - 1 );
	
	}
	
	var sViewer = "_sViewer=" + strViwerName;
	var sReportname =  " _sReportname=connection.reportname=/BusinessRegist/" + strViwerName + ".ozr";
	var sOdiNames = " _sOdiNames=odi.odinames=Q)RegCertificate";
	var	spCount = " _spCount=odi.Q)RegCertificate.pcount=4";
	var sParam = " _sParam=odi.Q)RegCertificate.args1=TMP_WRT_NUM=" + _sKEY1 + "||";
	sParam += "odi.Q)RegCertificate.args2=SEQ_IN_PARAM=" + strWhereInParam + "||";
	sParam += "odi.Q)RegCertificate.args3=SHOW_BORDER=" + strShowBorder + "||";
	sParam += "odi.Q)RegCertificate.args4=ROW_POS=" + strRowPos;
	var sRet = fnCreateDialogResize("bas::frmReport.xml", sViewer + sReportname + sOdiNames + spCount + sParam);

	
}
