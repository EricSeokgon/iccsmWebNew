/**
 1. 함수명 : fnInitButtonBar
 2. 기  능 : 화면상단 버튼상태 설정
 3. Params 
    없음
 4. Return
    없음
*/
function fnInitButtonBar()
{
	if (global.gv_WorkFlag == global.gv_INQUIRY)
	{
		fnAllControlState(false, this);
		
		divButtonBar.btnInsert.enable = false;
		divButtonBar.btnModify.enable = true;
		divButtonBar.btnRemove.enable = false;
		
		divButtonBar.btnSave.enable = fnSetMenuSaveButtonAuth(3, false);
		divButtonBar.btnCancel.enable = false;
		divButtonBar.btnReport.enable = true;
		divButtonBar.btnExcel.enable = true;		
	}
	else if (global.gv_WorkFlag == global.gv_INSERT)
	{
		fnAllControlState(false, this);
		
		divButtonBar.btnInsert.enable = false;
		divButtonBar.btnModify.enable = false;
		divButtonBar.btnRemove.enable = false;
		
		divButtonBar.btnSave.enable = fnSetMenuSaveButtonAuth(2, true);
		divButtonBar.btnCancel.enable = true;
		divButtonBar.btnReport.enable = false;
		divButtonBar.btnExcel.enable = false;
		
		
		fnControlState(true, this.tbMain.tpBase, "REC_NUM");
		tbMain.tpBase.btnReceiptNum.Enable = true;
	}
	else
	{
		fnAllControlState(true, this);
		
		divButtonBar.btnInsert.enable = false;
		divButtonBar.btnModify.enable = false;
		divButtonBar.btnRemove.enable = false;
		
		divButtonBar.btnSave.enable = fnSetMenuSaveButtonAuth(2, true);
		divButtonBar.btnCancel.enable = true;
		divButtonBar.btnReport.enable = false;
		divButtonBar.btnExcel.enable = false;
		
		
		fnControlState(false, this.tbMain.tpBase, "REC_NUM");
		tbMain.tpBase.btnReceiptNum.Enable = false;
	}
}

/**
 1. 함수명 : fnTablesBinding
 2. 기  능 : 검색조건에 대한 검색
 3. Params 
    없음
 4. Return
    없음
*/
function fnTablesBinding()
{
	// 2010-08-13 jeon 저장 후 _sKEY1값이 사라짐 수정
   if(_sKEY1 == ""){
		_sKEY1 = dsPT_R_ASSI_TRANS_STATEMENT.GetColumn(0,"RECV_NUM");
    }
        
	var sArgs = "CHECK=FALSE RECV_NUM=" + quote(_sKEY1) + " scSIDO_CODE=" + global.g_Sd_cd;
	var sTid = "SEARCH";
	var sUrl = "ICCS::cm/CmAction.do?cmd=TransReceiptMgrC";
	var sInDs = "";
	var sOutDs = "dsPT_R_ASSI_TRANS_STATEMENT=output dsPT_R_MEMO=output2 dsPT_R_DEFICIT=output3 dsdsCOMPANY_MASTER=output4" ;
	var sCallBack = "fn_CallBack";
	
	Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
}

function fn_CallBack(Svcid, sErrCode, sErrMsg) 
{
	http.sync = false;
	
	if (sErrCode < 0) 	
	{
		if (svcid == "SEARCH")
		{
			alert('데이터 조회 중 오류가 발생하였습니다.');
			return;
		}
		if (svcid == "SAVE")
		{
			alert('데이터 저장 중 오류가 발생하였습니다.');
			return;
		}
	}
	
	if (svcid == "CHECK_KEY")
	{
		if (dsCheckKey.rowcount > 0)
		{
			alert('존재하는 민원접수번호입니다.');
			tbMain.tpBase.REC_NUM.Text = "";
			tbMain.tpBase.REC_NUM.SetFocus();
		}
		else
		{
			
			var sArgs = "workMODE=MAIN";
			var sTid = "INSERT_SAVE";
			var sUrl = "ICCS::cm/CmAction.do?cmd=TransReceiptMgrCCUD";
			var sInDs = "dsPT_R_ASSI_TRANS_STATEMENT=dsPT_R_ASSI_TRANS_STATEMENT:u";
			var sOutDs = "dsPT_R_ASSI_TRANS_STATEMENT=output";
			var sCallBack = "fn_CallBack";
			
			Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
		}
	}
	if (svcid == "SAVE")
	{
		alert('저장되었습니다.');
		fnModify();
	}
	
	if (svcid == "INSERT_SAVE")
	{
		global.gv_WorkFlag = global.gv_MODIFY;
		fnInitButtonBar();
		
		dsPT_R_ASSI_TRANS_STATEMENT.ApplyChange();
		
		var sGroup;
		var nIndex = 0;
		
		for(var nLoop = 0; nLoop < dsPT_R_DEFICIT_CODE.rowcount; nLoop++)
		{
			sGroup = dsPT_R_DEFICIT_CODE.GetColumn(nLoop, "DEFI_GROUP");
			
			if (sGroup == "GUBU")
			{
				dsPT_R_DEFICIT.AddRow();
				dsPT_R_DEFICIT.SetColumn(nIndex, "RECV_NUM", tbmain.tpBase.REC_NUM.Text);
				dsPT_R_DEFICIT.SetColumn(nIndex, "DEFI_SEQ", nIndex + 1);
				dsPT_R_DEFICIT.SetColumn(nIndex, "SIDO_CODE", global.g_Sd_cd);
				dsPT_R_DEFICIT.SetColumn(nIndex, "DEFI_CODE", dsPT_R_DEFICIT_CODE.GetColumn(nLoop, "DEFI_CODE"));
				dsPT_R_DEFICIT.SetColumn(nIndex, "DEFI_GROUP", dsPT_R_DEFICIT_CODE.GetColumn(nLoop, "DEFI_GROUP"));
				dsPT_R_DEFICIT.SetColumn(nIndex, "DEFI_CONT", dsPT_R_DEFICIT_CODE.GetColumn(nLoop, "CONT"));
				
				nIndex++;
			}
		}

		var sArgs = "workMODE=ALL";
		var sTid = "SET_CODE";
		var sUrl = "ICCS::cm/CmAction.do?cmd=TransReceiptMgrCCUD";
		var sInDs = "dsPT_R_DEFICIT=dsPT_R_DEFICIT:u";
		var sOutDs = "dsPT_R_DEFICIT=output";
		var sCallBack = "fn_CallBack";
		
		Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
	}
		
	if (svcid == "MEMO_SAVE")
	{
		alert('저장되었습니다.');
		fnInitMemoGroup("INQUIRY");
	}
	if (svcid == "MEMO_DELETE")
	{
		alert('삭제되었습니다.');
		fnInitMemoGroup("INQUIRY");
	}
	if (svcid == "SEARCH")
	{
		tbMain.tpBase.TRAN_WRT_NUM.Readonly = true;
		tbMain.tpBase.TRAN_WRT_NUM.BKColor = "#dddddd";
	}
	
	if (svcid == "SET_CODE")
	{
		dsPT_R_DEFICIT.ApplyChange();
		tbMain.tpBase.TRAN_WRT_NUM.Readonly = true;
		tbMain.tpBase.TRAN_WRT_NUM.BKColor = "#dddddd";

	}
	
	if (svcid == "GET_CODE")
	{
		fnInitButtonBar();
		dsPT_R_ASSI_TRANS_STATEMENT.AddRow();
		dsPT_R_ASSI_TRANS_STATEMENT.SetColumn(0, "SIDO_CODE", global.g_Sd_cd);
		
		//tbMain.tpBase.REC_DATE.Value = Today();
		//tbMain.tpBase.STATE_PERIOD.Value = AddDate(Today(), 7); //양도양수 당일을 뺀 만 5일 (토일 제외)
		//wbjeon 2010-09-30 수정 
		tbMain.tpBase.REC_DATE.Value = Today();
		var thisToDay = tbMain.tpBase.REC_DATE.Value;			
		tbMain.tpBase.STATE_PERIOD.Value = fromDayAfter(thisToDay,5,60,"div");
		
		tbMain.tpBase.REC_INS_DT = Today();
		
		tbMain.tpBase.TRAN_WRT_NUM.Readonly = true;
		tbMain.tpBase.TRAN_WRT_NUM.BKColor = "#dddddd";
	}
	
	if (svcid == "GET_CORP")
	{
		dsPT_R_ASSI_TRANS_STATEMENT.SetColumn(0, "ASSI_TMP_WRT_NUM", 
							dsdsCOMPANY_MASTER.GetColumn(0, "TMP_WRT_NUM"));
		dsPT_R_ASSI_TRANS_STATEMENT.SetColumn(0, "TRAN_WRT_NUM", 
							dsdsCOMPANY_MASTER.GetColumn(0, "COI_WRT_NUM"));
		// 추가 함. 20100818 JUNG					
		dsPT_R_ASSI_TRANS_STATEMENT.SetColumn(0, "ASSI_NAME",dsdsCOMPANY_MASTER.GetColumn(0, "NAME"));
		dsPT_R_ASSI_TRANS_STATEMENT.SetColumn(0, "ASSI_TELNUM",dsdsCOMPANY_MASTER.GetColumn(0, "ADDR_TEL_NUM"));
		dsPT_R_ASSI_TRANS_STATEMENT.SetColumn(0, "ASSI_COMNUM",dsdsCOMPANY_MASTER.GetColumn(0, "MANA_NUM"));
		dsPT_R_ASSI_TRANS_STATEMENT.SetColumn(0, "ASSI_REP",dsdsCOMPANY_MASTER.GetColumn(0, "REP_NM_KOR"));
		dsPT_R_ASSI_TRANS_STATEMENT.SetColumn(0, "ASSI_ADDR",dsdsCOMPANY_MASTER.GetColumn(0, "ADDR_ADDR"));
		dsPT_R_ASSI_TRANS_STATEMENT.SetColumn(0, "ASSI_POST_NUM",dsdsCOMPANY_MASTER.GetColumn(0, "ADDR_POST_NUM"));
		dsPT_R_ASSI_TRANS_STATEMENT.SetColumn(0, "ASSI_DETAIL_ADDR",dsdsCOMPANY_MASTER.GetColumn(0, "ADDR_DETAIL_ADDR"));					
							
	}
}

