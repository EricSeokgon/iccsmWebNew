function fnValidation()
{
	var nIndex = 0;
	var tabIndex = 0;
	
	if (ToNumber(fromDayAfter(replace(tbMain.tpBase.REC_DATE.Value,'-',''),5,60,"div")) < ToNumber(replace(tbMain.tpBase.STATE_PERIOD.Value,"-",""))) 
	//if(fromDayAfter(tbMain.tpBase.PROC_LIM.Value,-5,60,"div") != replace(ToString(DateTime(tbMain.tpBase.RECV_DT.Value)),"-",""))
	{
		//dsError.AddRow();
		//dsError.SetColumn(nIndex, "FLD_NAME", "STATE_PERIOD");
		//dsError.SetColumn(nIndex, "TEXT", "민원처리일자가 14일을 초과합니다.");
		alert("민원처리일자가 5일을 초과합니다.");
		//nIndex++;
	}
	
	for(var nLoop = 0; nLoop < dsPT_R_DEFICIT.rowcount; nLoop++)
	{
		if(dsPT_R_DEFICIT.GetColumn(nLoop, "DEFI_OUT") != "1")
		{
			//nErrCnt++;
			dsError.AddRow();
			dsError.SetColumn(nIndex, "FLD_NAME", "grdPaper");
			dsError.SetColumn(nIndex, "TEXT", "흠결이 완료되지 않았습니다.");
			tabIndex = 1;
			nIndex++;
			break;
		}
	}
	
	if(fnEmpty(tbMain.tpBase.TRAN_NAME.Text))
	{
		dsError.AddRow();
		dsError.SetColumn(nIndex, "FLD_NAME", "TRAN_NAME");
		dsError.SetColumn(nIndex, "TEXT", "회사 상호가 입력되지 않았습니다.");
		
		nIndex++;
	}
	
	if(fnEmpty(tbMain.tpBase.TRAN_REP.Text))
	{
		dsError.AddRow();
		dsError.SetColumn(nIndex, "FLD_NAME", "TRAN_REP");
		dsError.SetColumn(nIndex, "TEXT", "대표자 성명이 입력되지 않았습니다.");
		
		nIndex++;
	}
	
	if(fnEmpty(tbMain.tpBase.TRAN_COMNUM.Value))
	{
		dsError.AddRow();
		dsError.SetColumn(nIndex, "FLD_NAME", "TRAN_COMNUM");
		dsError.SetColumn(nIndex, "TEXT", "사업자 등록번호가 입력되지 않았습니다.");
		
		nIndex++;
	}
	
	if (tbMain.tpBase.MANA_NUM.Value == tbMain.tpBase.TRAN_COMNUM.Value)
	{
		dsError.AddRow();
		dsError.SetColumn(nIndex, "FLD_NAME", "MANA_NUM");
		dsError.SetColumn(nIndex, "TEXT", "양수/양도 회사의 사업자 번호가 동일합니다.");
		nIndex++;
	}
	
	if (tbMain.tpBase.WRT_NUM.Text != tbMain.tpBase.TRAN_WRT_NUM.Text)
	{
		dsError.AddRow();
		dsError.SetColumn(nIndex, "FLD_NAME", "MANA_NUM");
		dsError.SetColumn(nIndex, "TEXT", "양수/양도 회사의 공사업 등록번호가 다릅니다.");
		nIndex++;
	}
	
	if (nIndex == 0) dsPT_R_ASSI_TRANS_STATEMENT.SetColumn(0, "DEFI_STE", "O");
	else dsPT_R_ASSI_TRANS_STATEMENT.SetColumn(0, "DEFI_STE", "X");
	
	if (nIndex > 0)
	{
		var nRet = fnCreateDialog("cm::frmErrorDLG.xml", "");
		if (nRet < 0) return true;
		else 
		{
			fnFindComponent(this, dsError.GetColumn(nRet, "FLD_NAME"));
			return false;
		}
	}
	
	return true;
}


