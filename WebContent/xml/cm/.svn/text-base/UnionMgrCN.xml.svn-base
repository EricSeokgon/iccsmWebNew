function fnCompanyDetailButton_Click(obj)
{
	var sDtWorkFlag;

	if(obj == tbMain.tbInfo.btnCInsert)	
	{
		var _sKEY1 = dsPT_R_M_UNION_STATEMENT.GetColumn(0, "TMP_WRT_NUM");

		sDtWorkFlag = global.gv_INSERT;
		dsPT_R_SUBSIDIARY_TEMP.AddRow();		
		dsPT_R_SUBSIDIARY_TEMP.SetColumn(dsPT_R_SUBSIDIARY_TEMP.row, "TMP_WRT_NUM", _sKEY1);
		dsPT_R_SUBSIDIARY_TEMP.SetColumn(dsPT_R_SUBSIDIARY_TEMP.row, "RECV_NUM", _sKEY2);
		dsPT_R_SUBSIDIARY_TEMP.SetColumn(dsPT_R_SUBSIDIARY_TEMP.row, "SIDO_CODE",  global.g_Sd_cd);
		dsPT_R_SUBSIDIARY_TEMP.SetColumn(dsPT_R_SUBSIDIARY_TEMP.row, "SEQ", dsPT_R_SUBSIDIARY_TEMP.rowcount);
		dsPT_R_SUBSIDIARY_TEMP.SetColumn(dsPT_R_SUBSIDIARY_TEMP.row, "SUB_PROC", "U");
		fnInitCompanyGroup(sDtWorkFlag);
	}
	else if (obj == tbMain.tbInfo.btnCModify)
	{
		sDtWorkFlag = global.gv_MODIFY;
		fnInitCompanyGroup(sDtWorkFlag);
	}
	else if (obj == tbMain.tbInfo.btnCRemove)
	{
		sDtWorkFlag = global.gv_INQUIRY;
		
		dsPT_R_SUBSIDIARY_TEMP.ApplyChange();
		var nCount = fnGridRowRemove(dsPT_R_SUBSIDIARY_TEMP);
		
		if(nCount == 0)
		{
			alert('삭제할 데이터를 선택하세요.');
			return;
		}
		var sArgs = "workMODE=SUBSIDIARY";	
		var sTid = "D_SUBSIDIARY";
		var sUrl = "ICCS::cm/CmAction.do?cmd=UnionMgrCCUDN";
		var sInDs = "dsPT_R_SUBSIDIARY_TEMP=dsPT_R_SUBSIDIARY_TEMP:u";
		var sOutDs = "";
		var sCallBack = "fn_CallBack";

		Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
	}
	else if (obj == tbMain.tbInfo.btnCCancel)
	{
		if (dsPT_R_SUBSIDIARY.GetRowType(dsPT_R_SUBSIDIARY.row) == "insert")
			dsPT_R_SUBSIDIARY.DeleteRow(dsPT_R_SUBSIDIARY.row);
		else
			fnRejectChange(dsPT_R_SUBSIDIARY);
		
		sDtWorkFlag = global.gv_INQUIRY;
		fnInitCompanyGroup(sDtWorkFlag);		
	}
	else if (obj == tbMain.tbInfo.btnCSave)
	{
			if( fnEmpty(dsPT_R_SUBSIDIARY_TEMP.GetColumn(dsPT_R_SUBSIDIARY_TEMP.row, "SUB_WRT_NUM")) ) {
			alert( "등록번호를 입력하세요" );
			return;
		}
		
		 //겹업 중복체크 석곤 2011-06-29
    	var rtn;
    	for(i=0;i<dsPT_R_SUBSIDIARY_TEMP.count;i++){
	    rtn = dsPT_R_SUBSIDIARY_TEMP.SearchRow(" SUB_WRT_NUM = '" + dsPT_R_SUBSIDIARY_TEMP.GetColumn(i,"SUB_WRT_NUM")+ "' &&  SUB_NAME = '" + dsPT_R_SUBSIDIARY_TEMP.GetColumn(i,"SUB_NAME")+ "'",i+1,dsPT_R_SUBSIDIARY_TEMP.count);
          if(rtn != -1 ){ //같은 값이 있을때
            alert("중복 입력 하였습니다");
            dsPT_R_SUBSIDIARY_TEMP.DeleteRow(dsPT_R_SUBSIDIARY_TEMP.row);
            
        	sDtWorkFlag = global.gv_INQUIRY;
		    fnInitCompanyGroup(sDtWorkFlag);
            
            return false;
            break;
           }
         }
		var sArgs = "workMODE=SUBSIDIARY";	
		var sTid = "I_SUBSIDIARY";
		var sUrl = "ICCS::cm/CmAction.do?cmd=UnionMgrCCUDN";
		var sInDs = "dsPT_R_SUBSIDIARY_TEMP=dsPT_R_SUBSIDIARY_TEMP:u";
		var sOutDs = "";
		var sCallBack = "fn_CallBack";
		
		Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
	}
}

