function fnCompanyAddButton_Click(obj)
{
	var sDtWorkFlag;
	
	if(obj == tbMain.tbAC.btnCInsert)	
	{
		sDtWorkFlag = global.gv_INSERT;
		dsPT_R_UNION_COMPANY_BEFORE.AddRow();
		dsPT_R_UNION_COMPANY_BEFORE.SetColumn(dsPT_R_UNION_COMPANY_BEFORE.row, "RECV_NUM", this.RECV_NUM.value);
		dsPT_R_UNION_COMPANY_BEFORE.SetColumn(dsPT_R_UNION_COMPANY_BEFORE.row, "SIDO_CODE", global.g_Sd_cd);
		fnInitCompanyAdd(sDtWorkFlag);		
	}
	else if (obj == tbMain.tbAC.btnCModify)
	{
		sDtWorkFlag = global.gv_MODIFY;
		fnInitCompanyAdd(sDtWorkFlag);
	}
	else if (obj == tbMain.tbAC.btnCRemove)
	{
		sDtWorkFlag = global.gv_INQUIRY;
		
		var oldSIDO_CODE = dsPT_R_M_UNION_STATEMENT.GetColumn(0,"SIDO_CODE");
		
		if (dsPT_R_UNION_COMPANY_BEFORE.GetColumn(_sSEL_UNION,"TMP_WRT_NUM") == dsPT_R_M_UNION_STATEMENT.GetColumn(0,"TMP_WRT_NUM"))
		{
			if (Confirm("합병 후 존속 또는 설립된 법인의 등록업체를 삭제 하시겠습니까"))
			{
				dsPT_R_M_UNION_STATEMENT.DeleteAll();
				dsPT_R_SUBSIDIARY_TEMP.DeleteAll();
				
			} else {
				return;
			}
		}
		
		dsPT_R_UNION_COMPANY_BEFORE.ApplyChange();
		var nCount = fnGridRowRemove(dsPT_R_UNION_COMPANY_BEFORE);
		
		if(nCount == 0)
		{
			alert('삭제할 데이터를 선택하세요.');
			return;
		}
		
		// 기술자 정보 임시데이터셋 초기화
		dsENGINEER_CHANGE_TEMP.ApplyChange();
		dsENGINEER_CHANGE_TEMP.DeleteAll();		
		
		http.Sync = true;
		var sArgs = "workMODE=D_UNION_COMPANY scRECV_NUM='"+_sKEY2+"' scSIDO_CODE='"+oldSIDO_CODE+"'";	
		var sTid = "D_COMPANY";
		var sUrl = "ICCS::cm/CmAction.do?cmd=UnionMgrCCUDN";
		var sInDs = "dsPT_R_UNION_COMPANY_BEFORE=dsPT_R_UNION_COMPANY_BEFORE:u ";
		sInDs += "dsPT_R_M_UNION_STATEMENT=dsPT_R_M_UNION_STATEMENT:u ";
		sInDs += "dsPT_R_SUBSIDIARY_TEMP=dsPT_R_SUBSIDIARY_TEMP:u ";
		var sOutDs = "";
		var sCallBack = "fn_CallBack";		
		Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
		http.Sync = false;		

	}
	else if (obj == tbMain.tbAC.btnCCancel)
	{
		if (dsPT_R_UNION_COMPANY_BEFORE.GetRowType(dsPT_R_UNION_COMPANY_BEFORE.row) == "insert")
			dsPT_R_UNION_COMPANY_BEFORE.DeleteRow(dsPT_R_UNION_COMPANY_BEFORE.row);
		else
			fnRejectChange(dsPT_R_UNION_COMPANY_BEFORE);
		
		sDtWorkFlag = global.gv_INQUIRY;
		fnInitCompanyAdd(sDtWorkFlag);		
	}
	else if (obj == tbMain.tbAC.btnCSave)
	{	
	
	var sTMP_WRT_NUM = dsPT_R_UNION_COMPANY_BEFORE.GetColumn(dsPT_R_UNION_COMPANY_BEFORE.row, "TMP_WRT_NUM");
	if(fnEmpty(sTMP_WRT_NUM)){
	dsPT_R_UNION_COMPANY_BEFORE.SetColumn(dsPT_R_UNION_COMPANY_BEFORE.row, "TMP_WRT_NUM", "000000");
	}
		var sArgs = "workMODE=UNION_COMPANY";	
		var sTid = "I_COMPANY";
		var sUrl = "ICCS::cm/CmAction.do?cmd=UnionMgrCCUDN";
		var sInDs = "dsPT_R_UNION_COMPANY_BEFORE=dsPT_R_UNION_COMPANY_BEFORE:u";
		var sOutDs = "";
		var sCallBack = "fn_CallBack";
		
		Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
		
		/**
		* 		보유 기술자 저장
		*/				
		var e_cnt = dsENGINEER_CHANGE_TEMP.count;
		var ed_cnt = dsPT_R_ENGINEER_CHANGE_TEMP.count;
		
		if (e_cnt == ""){e_cnt = 0;}
		if (ed_cnt == ""){ed_cnt = 0;}

		if (e_cnt > 1){
			for (var eng_i = 0; eng_i < e_cnt; eng_i++){						
				fnSetMasterDetailMulti_Add("dsENGINEER_CHANGE_TEMP", "dsPT_R_ENGINEER_CHANGE_TEMP",eng_i,ed_cnt);
				ed_cnt = ed_cnt + 1;
				//alert("1 "+eng_i+" 2 "+ed_cnt);
			}
			
			var sArgs = "workMODE=ENGINEER";
			var sTid = "ENGINEER_SAVE";
			var sUrl = "ICCS::cm/CmAction.do?cmd=UnionMgrCCUDN";
			var sInDs = "dsPT_R_ENGINEER_CHANGE_TEMP=dsPT_R_ENGINEER_CHANGE_TEMP:u";
			var sOutDs = "";
			var sCallBack = "fn_CallBack";
		
			Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
		}
		
		sDtWorkFlag = global.gv_INQUIRY;
		fnInitCompanyAdd(sDtWorkFlag);	
	}
}

function tbMain_tpBase_grdUnion_OnCellClick(obj,nRow,nCell,nX,nY,nPivotIndex)
{	
	if (nCell <> 0){
		if(dsPT_R_UNION_COMPANY_BEFORE.getColumn(nRow,"PCOM_SELECT") == "1")
		{
			for (var i = 0; i <= dsPT_R_UNION_COMPANY_BEFORE.RowCount(); i++){
				dsPT_R_UNION_COMPANY_BEFORE.setColumn(i,"PCOM_SELECT",0);
			}
		} else {
			for (var i = 0; i <= dsPT_R_UNION_COMPANY_BEFORE.RowCount(); i++){
				
				if (i == nRow){
					dsPT_R_UNION_COMPANY_BEFORE.setColumn(i,"PCOM_SELECT",1);
				} else {
					dsPT_R_UNION_COMPANY_BEFORE.setColumn(i,"PCOM_SELECT",0);
				}
			}	
		}
	}
	_sSEL_UNION = nRow;
}

