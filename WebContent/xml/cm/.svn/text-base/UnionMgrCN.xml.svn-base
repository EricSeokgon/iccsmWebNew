function fnInitCompanyAdd(sDtWorkFlag)
{

	if(sDtWorkFlag == global.gv_INQUIRY)
	{
		fnControlState(false, tbMain.tbAC, "btnCorp");
		fnControlState(true, tbMain.tbAC, "grdUnion");
		fnControlState(false, tbMain.tbAC, "A_COI_WRT_NUM, A_NAME, A_REP_NM_KOR,A_COM_NUM ,A_ADDR_TEL_NUM, A_REP_NM_HAN, A_MANA_NUM, A_ADDR_POST_NUM, A_ADDR_ADDR, A_ADDR_DETAIL_ADDR ");
		
		tbMain.tbAC.btnCInsert.Enable = true;
		tbMain.tbAC.btnCModify.Enable = true;
		tbMain.tbAC.btnCRemove.Enable = true;
		tbMain.tbAC.btnCSave.Enable = false;
		tbMain.tbAC.btnCCancel.Enable = false;
	}
	else if(sDtWorkFlag == global.gv_INSERT)
