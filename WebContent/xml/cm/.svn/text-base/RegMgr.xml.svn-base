{
	var sCode = fnCreateDialog("cm::RegMgrReportDlg.xml", "");
	
	var sKEY1 = dsPT_R_COMPANY_MASTER.GetColumn(dsPT_R_COMPANY_MASTER.row, "TMP_WRT_NUM");
	
	//내부건의서
	if (sCode == "RPT1")
	{
		var sViewer = "_sViewer=NewRegInnerR";
