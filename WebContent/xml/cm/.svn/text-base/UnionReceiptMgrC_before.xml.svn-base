	<Script><![CDATA[/*
1. 파일명 : UnionReceiptMgrC.xml
2. 작성일 : 2009-04-02
3. 작성자 : Y.D. JEONG
4. 기  능 : 합병 기본정보 입력
5. 변경이력
  가. 2009-04-02 최초작성 (Y.D. JEONG)
*/

#include "UserLib::CommonJs.js"
//////////////////////////////////////////////////////////////////
// 전역변수
//////////////////////////////////////////////////////////////////
var	_sKEY1;
var _sWorkFlag;

//////////////////////////////////////////////////////////////////
// Form Load 이벤트
//////////////////////////////////////////////////////////////////
function fnForm_Loaded(obj)
{
	global.gv_WorkFlag = _sWorkFlag;

	if(global.gv_WorkFlag == global.gv_INQUIRY)
	{
		fnInquiry();
	}
	else if(global.gv_WorkFlag == global.gv_INSERT)
	{
		fnInsert();
	}
	else if(global.gv_WorkFlag == global.gv_MODIFY)
	{
		fnModify();
	}
	
	
	fnInitMemoGroup("INQUIRY");
	
	divButtonBar.Image1.text = space(6) + "공사업체 합병 접수";
}

function fnForm_Activate(obj)
{
	global.g_activeFrm = this.Id;
}

//////////////////////////////////////////////////////////////////
// 공통기능
//////////////////////////////////////////////////////////////////
/**
 1. 함수명 : fnInitButtonBar
 2. 기  능 : 화면상단 버튼상태 설정
 3. Params 
    없음
 4. Return
    없음
*/
function fnInitButtonBar()
{
	if (global.gv_WorkFlag == global.gv_INQUIRY)
	{
		fnAllControlState(false, this);
		divButtonBar.btnInsert.enable = false;
		divButtonBar.btnModify.enable = true;
		divButtonBar.btnRemove.enable = false;
		divButtonBar.btnSave.enable = fnSetMenuSaveButtonAuth(3, false);
		divButtonBar.btnCancel.enable = false;
		divButtonBar.btnReport.enable = true;
		divButtonBar.btnExcel.enable = true;		
		divButtonBar.btnList.enable = true;		
	}
	else if (global.gv_WorkFlag == global.gv_INSERT)
	{
		fnAllControlState(false,this);
		
		divButtonBar.btnInsert.enable = false;
		divButtonBar.btnModify.enable = false;
		divButtonBar.btnRemove.enable = false;
		
		divButtonBar.btnSave.enable = fnSetMenuSaveButtonAuth(2, true);
		divButtonBar.btnCancel.enable = true;
		divButtonBar.btnReport.enable = false;
		divButtonBar.btnExcel.enable = false;
		
		fnControlState(true, this.tbMain.tpBase,"btnReceiptNum");
		fnControlState(true, this.tbMain.tpBase,"RECV_NUM");
		fnControlState(true, this.tbMain.tpBase,"btnGetNUM1");
		fnControlState(true, this.tbMain.tpBase,"btnGetNUM2");
	}
	else
	{
		fnAllControlState(true, this);
		divButtonBar.btnInsert.enable = false;
		divButtonBar.btnModify.enable = false;
		divButtonBar.btnRemove.enable = false;
		
		divButtonBar.btnSave.enable = fnSetMenuSaveButtonAuth(2, true);
		divButtonBar.btnCancel.enable = true;
		divButtonBar.btnReport.enable = false;
		divButtonBar.btnExcel.enable = false;
		
		fnControlState(false, this.tbMain.tpBase, "RECV_NUM");
	}
}

/**
 1. 함수명 : fnTablesBinding
 2. 기  능 : 검색조건에 대한 검색
 3. Params 
    없음
 4. Return
    없음
*/
function fnTablesBinding()
{
	var sArgs = "CHECK=MODIFY  RECV_NUM=" + quote(_sKEY1) + " scSIDO_CODE=" + global.g_Sd_cd;
	var sTid = "SEARCH";
	var sUrl = "ICCS::cm/CmAction.do?cmd=UnionReceiptMgrC";
	var sInDs = "";
	var sOutDs = "dsPT_R_DEFICIT=dsPT_R_DEFICIT dsPT_R_UNION_STATEMENT=dsPT_R_UNION_STATEMENT";
		sOutDs +=" dsPT_R_MEMO=dsPT_R_MEMO dsPT_ACOMPANY=dsPT_ACOMPANY dsPT_PCOMPANY=dsPT_PCOMPANY" ;
	var sCallBack = "fn_CallBack";
	
	Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
}

function fn_CallBack(Svcid, sErrCode, sErrMsg) 
{
	http.sync = false;
	
	if (sErrCode < 0) 	
	{
		if (svcid == "SEARCH")
		{
			alert('데이터 조회 중 오류가 발생하였습니다.');
			return;
		}
		if (svcid == "SAVE")
		{
			alert('데이터 저장 중 오류가 발생하였습니다.');
			return;
		}
		if(svcid == "MEMO_SAVE")
		{
			alert('메모 저장 중 오류가 발생하였습니다.');
			return;
		}
	}
	
	if (svcid == "CHECK_KEY")
	{
		if (dsCheckKey.rowcount > 0)
		{
			alert('존재하는 민원접수번호입니다.');
			tbMain.tpBase.RECV_NUM.Text = "";
			tbMain.tpBase.RECV_NUM.SetFocus();
		}
		else
		{
			dsPT_R_UNION_STATEMENT.SetColumn(0, "SIDO_CODE", global.g_Sd_cd);
			
			var sArgs = "workMODE=CHECK";	
			var sTid = "INSERT_SAVE";
			var sUrl = "ICCS::cm/CmAction.do?cmd=UnionReceiptMgrCCUD";
			var sInDs = "dsPT_R_UNION_STATEMENT=dsPT_R_UNION_STATEMENT:u";
			var sOutDs = "";
			var sCallBack = "fn_CallBack";
			
			Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
		}
	}
	if (svcid == "SAVE")
	{
		alert('저장되었습니다.');
		fnInquiry();
	}
	
	if (svcid == "GET_CODE")
	{
		fnInitButtonBar();	
		dsPT_R_UNION_STATEMENT.AddRow();
				
		tbMain.tpBase.RECV_DT.Value = Today();
		var thisToDay = tbMain.tpBase.RECV_DT.Value;			
		tbMain.tpBase.PROC_LIM.Value = fromDayAfter(thisToDay,5,60,"div");
	}
		
	if(svcid == "GET_NUM1")
	{
		//dsPT_R_UNION_STATEMENT.SetColumn(0,"UNIO_TMP_WRT_NUM_1",dsPT_ACOMPANY.GetColumn(0,"COI_WRT_NUM"));
		dsPT_R_UNION_STATEMENT.SetColumn(0,"ASC_TMP_WRT_NUM",dsPT_ACOMPANY.GetColumn(0,"TMP_WRT_NUM"));
	}	
	
	if( svcid == "GET_NUM2")
	{
		//dsPT_R_UNION_STATEMENT.SetColumn(0,"UNIO_TMP_WRT_NUM_2",dsPT_PCOMPANY.GetColumn(0,"COI_WRT_NUM"));
		dsPT_R_UNION_STATEMENT.SetColumn(0,"HANDED_TMP_WRT_NUM",dsPT_PCOMPANY.GetColumn(0,"TMP_WRT_NUM"));
	}
		
	if (svcid == "INSERT_SAVE")
	{
		global.gv_WorkFlag = global.gv_MODIFY;
		
		fnInitButtonBar();		
		var sGroup;
		var nIndex = 0;
		
		this.tbMain.tpBase.RECV_DT.Value=TODAY();
		//this.tbMain.tpBase.PROC_LIM.Value=AddDate(TODAY(), +7); //합병 당일을 뺀 만 5일 (토일 제외)
		//wbjeon 2010-09-30 수정 
		var thisToDay = this.tbMain.tpBase.RECV_DT.Value;			
		this.tbMain.tpBase.PROC_LIM.Value = fromDayAfter(thisToDay,5,60,"div");
				
		
		for(var nLoop = 0; nLoop < dsPT_R_DEFICIT_CODE.rowcount; nLoop++)
		{
			sGroup = dsPT_R_DEFICIT_CODE.GetColumn(nLoop, "DEFI_GROUP");
			
			if (sGroup == "GUBU")
			{
				dsPT_R_DEFICIT.AddRow();
				dsPT_R_DEFICIT.SetColumn(nIndex, "RECV_NUM", tbmain.tpBase.RECV_NUM.Text);
				dsPT_R_DEFICIT.SetColumn(nIndex, "DEFI_SEQ", nIndex + 1);
				dsPT_R_DEFICIT.SetColumn(nIndex, "SIDO_CODE", global.g_Sd_cd);
				dsPT_R_DEFICIT.SetColumn(nIndex, "DEFI_CODE", dsPT_R_DEFICIT_CODE.GetColumn(nLoop, "DEFI_CODE"));
				dsPT_R_DEFICIT.SetColumn(nIndex, "CONT", dsPT_R_DEFICIT_CODE.GetColumn(nLoop, "CONT"));
				dsPT_R_DEFICIT.SetColumn(nIndex, "DEFI_GROUP", dsPT_R_DEFICIT_CODE.GetColumn(nLoop, "DEFI_GROUP"));
				
				nIndex++;
			}
		}	
			
		var sArgs = "workMODE=ALL";
		var sTid = "SET_CODE";
		var sUrl = "ICCS::cm/CmAction.do?cmd=UnionReceiptMgrCCUD";
		var sInDs = "dsPT_R_DEFICIT=dsPT_R_DEFICIT:u";
		var sOutDs = "dsPT_R_DEFICIT=output";
		var sCallBack = "fn_CallBack";
		
		Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
	}
	

	if (svcid == "MEMO_SAVE")
	{
		alert('저장되었습니다.');
		fnInitMemoGroup("INQUIRY");
	}
	if (svcid == "MEMO_DELETE")
	{
		alert('삭제되었습니다.');
		fnInitMemoGroup("INQUIRY");
	}
	
	if (svcid == "SET_CODE")
	{
		dsPT_R_DEFICIT.ApplyChange();
	}
}

