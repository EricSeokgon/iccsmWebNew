	}
	
	// 2010.04.27
	// 피합병업체 2곳중 1곳을 선택하는게 아닌 제3의 신생업체가 생성되는 부분은 협의가 필요하다.
	// 우선 접수시 제3의 신생업체 부분을 막는다.
	// UnionMgrC 에는 완벽치 않지만 신생업체 생성부분의 코드가 있다.
	
	// 존속법인등록번호 선택 했는지 여부
	if( fnEmpty( dsPT_R_UNION_STATEMENT.GetColumn( 0, "ESTA_TMP_WRT_NUM" ) ) ) {
		dsError.AddRow();
		dsError.SetColumn(nIndex, "FLD_NAME", "ESTA_WRT_NUM");
		dsError.SetColumn(nIndex, "TEXT", "존속법인이 선택되지 않았습니다.");
		nIndex++;
	}
	
	if (nIndex == 0) dsPT_R_UNION_STATEMENT.SetColumn(0, "DEFI_STE", "O");
	else dsPT_R_UNION_STATEMENT.SetColumn(0, "DEFI_STE", "X");
	
	if (nIndex > 0)
	{
		var nRet = fnCreateDialog("cm::frmErrorDLG.xml", "");
		if (nRet < 0) return true;
		else 
		{
			fnFindComponent(this, dsError.GetColumn(nRet, "FLD_NAME"));
			return false;
		}
	}
	
	return true;
}


