function fnCancel()
{
	if(!Confirm('취소하시겠습니까?')) return;
	
	if(global.gv_WorkFlag == global.gv_INQUIRY)
	{
		//키값을 자동으로 생성할 경우 삭제하는 로직을 추가해주세요
		Close();
	}
	else
	{
		fnRejectChange(dsPT_R_UNION_STATEMENT);
		fnRejectChange(dsPT_R_MEMO);
		//dsPT_R_DEFICIT.DeleteAll();
		//dsPT_R_UNION_STATEMENT.DeleteAll();
		//dsPT_ACOMPANY.DeleteAll();
		//dsPT_PCOMPANY.DeleteAll();
		alert('취소되었습니다.');	
	}

	fnInquiry();
}

