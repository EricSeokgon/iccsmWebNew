		var sArgs = "workMODE=REFER_DUPLICATE_CHECK scTMP_WRT_NUM="+ quote(_sKEY1);
		var sTid = "REFER_DUPLICATE_CHECK";
		var sUrl = "ICCS::cm/CmAction.do?cmd=RegMgrC";
