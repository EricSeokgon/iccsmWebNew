			return;
		}
		//-- 종료 :: 대표자가 타업체 재직기술자인지 체크
		
		//-- 시작 :: 기술자가 타업체 대표자인지 체크
		http.Sync = true;
		
		dsREFER_DUPLICATE.DeleteAll();
		var sArgs = "workMODE=ENGINEER_DUPLICATE_CHECK scTMP_WRT_NUM="+ quote(_sKEY1);
		var sTid = "ENGINEER_DUPLICATE_CHECK";
		var sUrl = "ICCS::cm/CmAction.do?cmd=RegMgrC";
		var sInDs = "dsPT_R_ENGINEER_CHANGE=dsPT_R_ENGINEER_CHANGE";
		var sOutDs = "dsREFER_DUPLICATE=output";
		var sCallBack = "fn_CallBack";
		
		Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
		http.Sync = false;
		
		if( dsREFER_DUPLICATE != null && dsREFER_DUPLICATE.rowcount > 0 ) {
			alert( dsREFER_DUPLICATE.GetColumn(0, "NM_KOR") + " 은(는) " + dsREFER_DUPLICATE.GetColumn(0, "NAME") + " 업체에 대표자로 재직 중입니다.\n" +
			 "\n기술자 정보를 확인하시기 바랍니다.\n데이터를 저장하지 않았습니다.");
			return;
		}
		//-- 종료 :: 기술자가 타업체 대표자인지 체크
		
		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COI_WRT_NUM")))
		{
			alert('공사업등록번호가 입력되지 않았습니다.');

			MOT_STE.Value = "STD002";
			fnCheckMotSte();
		}
		
		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "PAY_CAP")))
		{
			alert('납입 자본금 정보가 입력되지 않았습니다.');
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		
		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "TUP_AOM")))
		{
			alert('자본금 예치 정보가 입력되지 않았습니다.');
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		
		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "REA_CAP")))
		{
			alert('실질 자본금 정보가 입력되지 않았습니다.');
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		
		var nAmt = toNumber(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "PAY_CAP"));
		var nRAmt = toNumber(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "REA_CAP"));
		var nTAmt = dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "TUP_AOM");
		
		
		if (dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COMMANA_CLASS") == "1" && nAmt < 150000)
		{
			var sMsg = "법인의 경우 납입 자본금이 150,000,000 이상 등록이 가능합니다\n\n";
				sMsg += "                등록을 완료할 수 없습니다.                ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		
		
		if (dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COMMANA_CLASS") == "1" && nRAmt < 150000)
		{
			var sMsg = "법인의 경우 실질 자본금이 150,000,000 이상 등록이 가능합니다\n\n";
				sMsg += "                등록을 완료할 수 없습니다.                ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		
		if (dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COMMANA_CLASS") == "2" && nRAmt < 200000)
		{
			var sMsg = "개인의 경우 실질 자본금이 200,000,000 이상 등록이 가능합니다\n\n";
				sMsg += "                등록을 완료할 수 없습니다.                ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		
		//if (dsPT_R_ENGINEER_CHANGE.rowcount < 4) 2009-10-09 
		if (dsPT_R_ENGINEER_CHANGE.rowcount <= 2)
		{
			var sMsg = "정보통신 기술자 보유숫자가 부족합니다.\n\n";
				sMsg += "    등록을 완료할 수 없습니다.     ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		else
		{
			var sEngVal = "X";
			for (var nLoop = 0; nLoop < dsPT_R_ENGINEER_CHANGE.rowcount; nLoop++)
			{
				var sGrade = dsPT_R_ENGINEER_CHANGE.GetColumn(nLoop, "TMP_FIELD");
				if (sGrade == "특급" || sGrade == "중급" || sGrade == "고급")
				{
					sEngVal = "O";
					break;
				}
			}
			
			if(sEngVal == "X")
			{
				var sMsg = "중급이상 기술자 보유숫자가 부족합니다.\n\n";
					sMsg += "    등록을 완료할 수 없습니다.     ";
				alert(sMsg);
				MOT_STE.Value = "STD002";
				fnCheckMotSte();
				return;
			}
		}
		
		if (dsPT_R_REFUSAL_REASON.rowcount > 0)
		{
			var sMsg = "등록 결격 사유가 존재합니다.\n\n";
					sMsg += "   등록을 완료할 수 없습니다. ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		
		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COMPANY_DIAG_ISSUE_DT")))
		{
			var sMsg = "기업진단보고서 발급일자가가 입력되지 않았습니다..\n\n";
					sMsg += "   등록을 완료할 수 없습니다. ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;		
		}
		
		if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COMPANY_DIAG_BAS_DT")))
		{
			var sMsg = "기업진단보고서 기준일자가가 입력되지 않았습니다.\n\n";
					sMsg += "   등록을 완료할 수 없습니다. ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;		
		}
		
		var sDay = dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "COMPANY_DIAG_BAS_DT"); 
		
		//if(Today() > AddDate(sDay, 30))
		// 2009-09-18 수정 방송통신위원회고시 제2009-16호 30일->45일변경
		//if(Today() > AddDate(sDay, 45)) 
		if(sDay > AddDate(sDay, 45)) 		
		{
			var sMsg = "기업진단보고서 기준일자가가 45일 이내에 발급되지 않았습니다.\n\n";
				sMsg += "                등록을 완료할 수 없습니다.                 ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;		
		}
		
		//  2009-10-28 20 -> 15제곱미터임 이상
		//if (fnEmpty(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "OFFICE_AREA")) 
		//	|| ToInteger(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "OFFICE_AREA")) < 20)
		if (ToInteger(trim(dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "OFFICE_AREA"))) <= 14)
		{
			var sMsg = "사무실 면적이 부족합니다.\n\n";
					sMsg += "등록을 완료할 수 없습니다. ";
			alert(sMsg);
			MOT_STE.Value = "STD002";
			fnCheckMotSte();
			return;
		}
		
		var wrtDt = TMP_WRT_DT.Value;
		
		if( fnEmpty(wrtDt) ) {
			alert( "등록완료일자를 입력해주십시오." );
			return;
		}
		
		var sReturn;
	
		if(this.MOT_STE.index == 1) {	
			var sArgs = " _parentType=Uni _chgCode=" + dsPT_R_BASIC_CHANGE_HISTORY.GetColumn(0, "EXT_CHG_CLASS_CODE");
