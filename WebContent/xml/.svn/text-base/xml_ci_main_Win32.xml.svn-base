function gfn_setCon(conFg)  
{
	g_conType = conFg;
	
	if (length(g_activeFrm) > 1) 
	{
		object(g_activeFrm).fn_query();
	}
}

function gfn_setMenu(mnuType) 
{
	if (mnuType == "TREE") 
	{
		global.MainMenu.div_treemenu.Visible = true;
		global.MainMenu.div_btnmenu.Visible = false;
	} 
	else 
	{
		if (mnuType == "BTN") 
		{
			global.MainMenu.div_treemenu.Visible = false;
			global.MainMenu.div_btnmenu.Visible = true;
		}
	}
}

function fnCallBack(svcid,errCode,errMsg)
{
	http.sync = false;
	
	g_Titel = gds_menulist.GetColumn(0,"frmtxt");
	fn_subHide();  
	fn_subShow();
	
	var arg = "scUSER_ID="+g_User_ID+"&scPASSWD="+g_passwd;
	//global.MainMenu.Microsoft0.Navigate("http://99.1.5.83/login/LoginAction.do?cmd=LoginLife&"+arg,"","","",arg);
	global.MainMenu.Microsoft0.Navigate( global.g_ext_iccs_url + "login/LoginAction.do?cmd=LoginLife&"+arg,"","","","");

}

function fn_subHide()
{  
	var i,obj;
	for(i=1;i< 10;i++){
		obj = object("global.MainMenu.sub_img"+i);
		obj.text = "";
		obj.Visible = true;
		obj.ImageID = "left_menu2_"+i;
	}	
}

function fn_subShow()
{	 
	for(i=1; i < gds_menulist.RowCount() ; i++){
		obj = object("global.MainMenu.sub_img"+(i));
		var sub_txt = gds_menulist.GetColumn(i,"frmtxt");
		obj.text = space(3) +sub_txt;
		obj.Visible = true;
		obj.userdata = gds_menulist.GetColumn(i,"frmurl");
		if(Pg_ID == gds_menulist.GetColumn(i,"frmid")){
			obj.ImageID = "left_menu2_"+(i)+"_on";
			obj.Color = "#FFFFFF";		
			global.MainMenu.Image2.text = g_Titel;
			title = sub_txt;
		}else{
			obj.ImageID = "left_menu2_"+(i);
			obj.Color = "#1c3c5a";
		}
	}
	
	for(j=gds_menulist.RowCount(); j < 9 ; j++)
	{
		obj = object("global.MainMenu.sub_img"+(j));
		obj.ImageID = "left_menu2_"+(j);
			obj.Color = "#1c3c5a";
	}
}

