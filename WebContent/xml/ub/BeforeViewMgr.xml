function fnForm_Loaded(obj)
{
	fnTablesBinding();
}

function fnTablesBinding()
{
	var sArgs = "workMODE=ALL";
	    sArgs += " scRECV_NUM=" + quote(scRECV_NUM);
	    sArgs += " scSIDO_CODE="+global.g_Sd_cd+" scSIGUNGU_CODE="+global.g_Sgg_cd;
	
	var sTid = "SEARCH";
	var sUrl = "ICCS::ub/UbAction.do?cmd=BeforeViewMgr";
	var sInDs = ""; 
	var sOutDs = "ds_before=output";
	var sCallBack = "fn_CallBack";
	
	Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
}

function fn_CallBack(Svcid, sErrCode, sErrMsg)
{
	if (sErrCode < 0) 	
	{
		if (svcid == "SEARCH")
		{
			alert('데이터 조회 중 오류가 발생하였습니다.');
		}
		if(svcid == "CODE")
		{
			alert('코드 조회 중 오류가 발생하였습니다.');
		}
		
		return;
	}
	

	if (svcid == "SEARCH")
	{
		fnAllControlState(false, this);
		fnControlState(true, this, "btnOk");
	}

	
	if (svcid == "DELETE")
	{
		alert("삭제 되었습니다.");
	}
	
}

//////////////////////////////////////////////////////////////////
// 검색 버튼 이벤트
//////////////////////////////////////////////////////////////////
