function fn_CallBack(Svcid, sErrCode, sErrMsg) 
{
	if (sErrCode < 0) 	
	{
		alert(sErrCode + " : " + sErrMsg);
		if (svcid == "SAVE")
		{
			alert('데이터 조회 중 오류가 발생하였습니다.');
		}

		return;
	}
	if (svcid == "SAVE")
	{
	     alert('저장되었습니다.');
    }

	if (svcid =="GET_DOC_NUM")
	{
	 var cntn = ds_PT_UB_CONSTRUCTION.rowcount();
     if( rownum<cntn){
	   _sKEY2 = dsDOC_NUM.GetColumn(dsDOC_NUM.row,"DOC_NUM1") + dsDOC_NUM.GetColumn(dsDOC_NUM.row,"DOC_NUM2");
     
      //ds_PT_UB_CONSTRUCTION.SetColumn(j,"CIV_RECV_NUM",_sKEY2);
      ds_PT_UB_CONSTRUCTION.SetColumn(rownum,"RECV_NUM",_sKEY2);	
      rownum++;
       if(rownum==cntn){
         return;
       }
      else
        fnKey();
      }
     }
}


