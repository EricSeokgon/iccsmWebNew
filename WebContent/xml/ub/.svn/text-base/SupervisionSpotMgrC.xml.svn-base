function fn_CallBack(Svcid, sErrCode, sErrMsg) 
{
	if (sErrCode < 0) 	
	{
		alert(sErrCode + " : " + sErrMsg);
		if (svcid == "SEARCH")
		{
			alert('데이터 조회 중 오류가 발생하였습니다.');
		}
		if (svcid == "DELETE")
		{
			alert('데이터 삭제 중 오류가 발생하였습니다.');
		}
		if (svcid == "MEMO_SAVE")
		{
			alert('메모 저장 중 오류가 발생하였습니다.');
		}
		return;
	}
	
	if (svcid == "DELETE")
	{
		alert('삭제되었습니다.');
		return;
	}
	
	if (svcid == "save")
	{
		alert('저장되었습니다.');
		global.gv_WorkFlag = global.gv_INQUIRY;
		fnInitButtonBar();
		return;
	}
	
	if (svcid == "MEMO_SAVE")
	{
		alert('메모가 저장되었습니다.');
