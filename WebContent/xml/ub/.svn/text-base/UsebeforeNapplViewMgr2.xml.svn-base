}

function fn_CallBack(Svcid, sErrCode, sErrMsg)
{
	if (sErrCode < 0) 	
	{
		if (svcid == "SEARCH")
		{
			alert('데이터 조회 중 오류가 발생하였습니다.');
		}
		if(svcid == "CODE")
		{
			alert('코드 조회 중 오류가 발생하였습니다.');
		}
		
		return;
	}
	

	if (svcid == "SEARCH")
	{
		fnAllControlState(false, this);
		fnControlState(true, this, "btnOk");
	}

	
	if (svcid == "DELETE")
	{
		alert("삭제 되었습니다.");
	}
	
}

