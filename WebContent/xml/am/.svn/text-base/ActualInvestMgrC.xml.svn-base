		if (sReturn == null || sReturn.length() == 0) return;

		_sKEY1 = sReturn[0];
		_sKEY2 = sReturn[1];

		sArgs = "workMODE=GET_NUM scWRT_NUM=" + _sKEY1 + " scTMP_WRT_NUM=" + _sKEY2;
		var sTid = "GET_NUM";
		var sUrl = "ICCS::am/AmAction.do?cmd=CancelMgrC";
		var sInDs = "";
		var sOutDs = "dsPT_M_MASTER=output";
		var sCallBack = "fn_CallBack";
		
		Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
	}
	
	if (obj == tbMain.tpBase.btnGetNUM)
	{
		if (fnEmpty(tbMain.tpBase.REPORT_CODE.Text))
		{
			alert('기술자 이름을 입력하세요.');
			return;
		}
		var sArgs = "_sWorkGroup=ActualInvestMgrC _sOldName=" + quote(tbMain.tpBase.REPORT_CODE.Text);
		
		var sReturn = fnCreateDialog("bas::ExperStD", sArgs);
		
		if (sReturn == null || sReturn.length() == 0) return;

		tbMain.tpBase.REPORT_CODE.Text = sReturn[1];
		dsPT_S_CAPABILLITY_ASSESSMENT.SetColumn(dsPT_S_CAPABILLITY_ASSESSMENT.row, "QUAL_NUM", sReturn[0]);
		dsPT_S_CAPABILLITY_ASSESSMENT.SetColumn(dsPT_S_CAPABILLITY_ASSESSMENT.row, "ADDR", sReturn[4]);
	}
}

function fnSetDetailDataset()
{
	fnSetMasterDetail("dsPT_R_COMPANY_MASTER", "dsPT_R_ADDRESS_CHANGE");
}

function fnSetMasterDetail(dsMaster, dsDetail)
{
	var sDColName;
	var sMColName;
	var sValue;
	
	var nMCount = object(dsMaster).colcount;
	var nDCount = object(dsDetail).colcount;
	
	if(object(dsDetail).rowcount == 0)
		object(dsDetail).addrow();
	
	for(var nLoop = 0; nLoop < nDCount; nLoop++)
	{
		sDColName = object(dsDetail).GetColID(nLoop);
		for(var nIndex = 0; nIndex < nMCount; nIndex++)
		{
			sMColName = object(dsMaster).GetColID(nIndex);
			if(trim(sDColName) == trim(sMColName))
			{
				sValue = object(dsMaster).GetColumn(0, sMColName);
				object(dsDetail).SetColumn(0, sDColName, sValue);
				break;
			}
		}
	}
}


