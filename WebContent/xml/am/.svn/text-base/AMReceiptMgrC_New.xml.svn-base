	if(!Confirm('선택하신 내용을 삭제하시겠습니까?')) return;
	
	dsPT_M_MASTER.ApplyChange();
	var nCount = fnGridRowRemove(dsPT_M_MASTER);
	
	if(nCount == 0)
	{
		alert('데이터 삭제 중 오류가 발생하였습니다.');
