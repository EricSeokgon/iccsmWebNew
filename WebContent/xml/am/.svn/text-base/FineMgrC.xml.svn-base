    없음
*/
function fnSave()
{

	if (dsPT_M_MASTER.GetColumn(0, "MOT_STE") == "C00003")
	{
		if(fnEmpty(dsPT_M_MASTER.GetColumn(0, "DISPO_DT")))
		{
			var sMessage = "행정처분 일자가 입력되지 않았습니다.\n\n";
				sMessage += "    처분을 완료할 수 없습니다.";
			alert(sMessage);
			dsPT_M_MASTER.SetColumn(0, "MOT_STE", "C00002");
			return;
		}
		
		if(fnEmpty(dsPT_M_MASTER.GetColumn(0, "DISPO_CAUSE")))
		{
			var sMessage = "행정처분 사유가 입력되지 않았습니다.\n\n";
				sMessage += "    처분을 완료할 수 없습니다.";
			alert(sMessage);
			dsPT_M_MASTER.SetColumn(0, "MOT_STE", "C00002");
			return;
		}
	}
	
	dsPT_M_PROSECUTION.SetColumn(dsPT_M_PROSECUTION.row,"PRSEC_DT",dsPT_M_MASTER.GetColumn(0, "DISPO_DT"));
	
	var sArgs = "workMODE=ALL";
	var sTid = "SAVE";
	var sUrl = "ICCS::am/AmAction.do?cmd=FineMgrCUD";
	var sInDs = "dsPT_M_MASTER=dsPT_M_MASTER:u dsPT_M_PROSECUTION=dsPT_M_PROSECUTION:u ";	
	var sOutDs = "dsPT_M_MASTER=output dsPT_M_PROSECUTION=output2 ";
	var sCallBack = "fn_CallBack";
	
	Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
}

