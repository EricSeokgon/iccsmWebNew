 4. Return
    없음
*/
function fnTablesBinding()
{
	var sArgs = "workMODE=StopMgrC scWRT_NUM=" + _sKEY1 + " scTMP_WRT_NUM=" + _sKEY2;
	var sTid = "SEARCH";
	var sUrl = "ICCS::am/AmAction.do?cmd=FineMgrC";
	var sInDs = "";
	var sOutDs  = "dsPT_M_PROSECUTION=output dsPT_M_MASTER=output2 " ;
		sOutDs += "dsPT_M_MEMO=output3 dsPT_M_BEF_REPORT=output4 " ;		
	var sCallBack = "fn_CallBack";
	
	Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
}

function fn_CallBack(Svcid, sErrCode, sErrMsg) 
{
	http.sync = false;
	
	if (sErrCode < 0) 	
	{
		if (svcid == "SEARCH")
		{
			alert('데이터 조회 중 오류가 발생하였습니다.');
			return;
		}
		else if (svcid == "SAVE")
		{
			alert('데이터 저장 중 오류가 발생하였습니다.');
			return;
		}
		if (svcid == "MEMO_SAVE")
		{
			alert('데이터 저장 중 오류가 발생하였습니다.');
			fnMemoButton_Click(tbMain.tpMemo.btnMemoCancel);
			fnInitMemoGroup("INQUIRY");
			return;
		}
		if (svcid == "MEMO_DELETE")
		{
			alert('데이터 삭제 중 오류가 발생하였습니다.');
			fnInitMemoGroup("INQUIRY");
			return;
		}
		if (svcid == "NOTI_SAVE")
		{
			alert('데이터 저장 중 오류가 발생하였습니다.');
			fnNotiButton_Click(tbMain.tpProc.btnNotiCancel);
			fnInitNotiGroup("INQUIRY");
			return;
		}
		if (svcid == "NOTI_DELETE")
		{
			alert('데이터 삭제 중 오류가 발생하였습니다.');
			fnInitNotiGroup("INQUIRY");
			return;
		}
	}
	
	if (svcid == "SAVE")
	{
		alert('저장되었습니다.');
		fnInquiry();
	}
	
	if (svcid == "INSERT_SAVE")
	{	
		global.gv_WorkFlag = global.gv_MODIFY;
		fnInitButtonBar();
		
		if (dsPT_TEMPLET.rowcount > 0)
		{
			var User_ID = global.g_User_ID;
			var Form_ID = "FineMgrC";
			var sArgs = "User_ID=" + User_ID + " Form_ID=" + Form_ID;
			var sReturn = fnCreateDialog("st::TempletOutput.xml", sArgs);
		}
		
		dsPT_M_PROSECUTION.SetColumn(0, "WRT_NUM", dsPT_M_MASTER.GetColumn(0, "WRT_NUM"));
		dsPT_M_PROSECUTION.SetColumn(0, "TMP_WRT_NUM", dsPT_M_MASTER.GetColumn(0, "TMP_WRT_NUM"));		
	}
	
	if (svcid == "MEMO_SAVE")
	{
		alert('저장되었습니다.');
		fnInitMemoGroup("INQUIRY");
	}
	if (svcid == "MEMO_DELETE")
	{
		alert('삭제되었습니다.');
		fnInitMemoGroup("INQUIRY");
	}
	
	if (svcid == "NOTI_SAVE")
	{
		alert('저장되었습니다.');
		fnInitNotiGroup("INQUIRY");
	}
	if (svcid == "NOTI_DELETE")
	{
		alert('삭제되었습니다.');
		fnInitNotiGroup("INQUIRY");
	}
	if(svcid == "SEARCH")
	{
		if (dsPT_M_PROSECUTION.rowcount ==0){
			dsPT_M_PROSECUTION.AddRow();
			dsPT_M_PROSECUTION.SetColumn(0, "WRT_NUM", dsPT_M_MASTER.GetColumn(0, "WRT_NUM"));
			dsPT_M_PROSECUTION.SetColumn(0, "TMP_WRT_NUM", dsPT_M_MASTER.GetColumn(0, "TMP_WRT_NUM"));
		}
		Combo1_OnChanged();
	}	
}

