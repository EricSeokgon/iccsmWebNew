*/

#include "UserLib::CommonJs.js"
//////////////////////////////////////////////////////////////////
// 전역변수
//////////////////////////////////////////////////////////////////
var	_sKEY1;
var	_sKEY2;
var _sWorkFlag;

//////////////////////////////////////////////////////////////////
// Form Load 이벤트
//////////////////////////////////////////////////////////////////
function fnForm_Loaded(obj)
{
	global.gv_WorkFlag = _sWorkFlag;
	fnGetCodeValue();
	Combo1_OnChanged();
		
	
	if(global.gv_WorkFlag == global.gv_INQUIRY)
	{
		fnInquiry();
	}
	else if(global.gv_WorkFlag == global.gv_INSERT)
	{
		global.gv_WorkFlag = global.gv_INSERT;
		fnInitButtonBar();
	}
	else if(global.gv_WorkFlag == global.gv_MODIFY)
	{
		fnModify();
	}
	
	fnInitMemoGroup("INQUIRY");
	fnInitNotiGroup("INQUIRY");
		
	divButtonBar.Image1.text = space(6) + "영업정지 및 과태료부과";
}

function fnForm_Activate(obj)
{
	global.g_activeFrm = this.Id;
}

//////////////////////////////////////////////////////////////////
// 공통기능
//////////////////////////////////////////////////////////////////
/**
 1. 함수명 : fnInitButtonBar
 2. 기  능 : 화면상단 버튼상태 설정
 3. Params 
    없음
 4. Return
    없음
*/
function fnInitButtonBar()
{
	if (global.gv_WorkFlag == global.gv_INQUIRY)
	{
		fnAllControlState(false, this);
		
		divButtonBar.btnInsert.enable = false;
		divButtonBar.btnModify.enable = true;
		divButtonBar.btnRemove.enable = false;
		
		divButtonBar.btnSave.enable = fnSetMenuSaveButtonAuth(2, false);
		divButtonBar.btnCancel.enable = false;
		divButtonBar.btnReport.enable = true;
		divButtonBar.btnExcel.enable = false;		
	}
	else if (global.gv_WorkFlag == global.gv_INSERT)
	{
		fnAllControlState(false, this);
		
		divButtonBar.btnInsert.enable = false;
		divButtonBar.btnModify.enable = false;
		divButtonBar.btnRemove.enable = false;
		
		divButtonBar.btnSave.enable = fnSetMenuSaveButtonAuth(2, true);
		divButtonBar.btnCancel.enable = true;
		divButtonBar.btnReport.enable = false;
		divButtonBar.btnExcel.enable = false;
	}
	else
	{
		fnAllControlState(true, this);
		
		divButtonBar.btnInsert.enable = false;
		divButtonBar.btnModify.enable = false;
		divButtonBar.btnRemove.enable = false;
		
		divButtonBar.btnSave.enable = fnSetMenuSaveButtonAuth(2, true);
		divButtonBar.btnCancel.enable = true;
		divButtonBar.btnReport.enable = false;
		divButtonBar.btnExcel.enable = false;
				
		fnControlState(false, this, "RC_REGIST_NUM,COM_NAME,COI_WRT_NUM,ADDR_POST_NUM,ADDR_ADDR,ADDR_DETAIL_ADDR,NEXT_DISPO");
	}
}


/**
 1. 함수명 : fnTablesBinding
 2. 기  능 : 검색조건에 대한 검색
 3. Params 
    없음
 4. Return
    없음
*/
function fnTablesBinding()
{
	var sArgs = "workMODE=StopIncidenceMgrC scWRT_NUM=" + _sKEY1 + " scTMP_WRT_NUM=" + _sKEY2;
	var sTid = "SEARCH";
	var sUrl = "ICCS::am/AmAction.do?cmd=StopIncidenceMgrC";
	var sInDs = "";
	var sOutDs  = "dsPT_M_SUSPENSION=output dsPT_M_MASTER=output2 " ;
		sOutDs += "dsPT_M_MEMO=output3 dsPT_M_BEF_REPORT=output4 " ;
		sOutDs += "dsPT_M_NEGLIGENCE=output5 " ;
					
	var sCallBack = "fn_CallBack";
	
	Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
}

function fn_CallBack(Svcid, sErrCode, sErrMsg) 
{
	http.sync = false;
	
	if (sErrCode < 0) 	
	{
		if (svcid == "SEARCH")
		{
			alert('데이터 조회 중 오류가 발생하였습니다.');
			return;
		}
		if (svcid == "SAVE")
		{
			alert('데이터 저장 중 오류가 발생하였습니다.');
			return;
		}
		if (svcid == "NOTI_SAVE")
		{
			alert('데이터 저장 중 오류가 발생하였습니다.');
			fnNotiButton_Click(tbMain.tpProc.btnNotiCancel);
			fnInitNotiGroup("INQUIRY");
			return;
		}
		if (svcid == "NOTI_DELETE")
		{
			alert('데이터 삭제 중 오류가 발생하였습니다.');
			fnInitNotiGroup("INQUIRY");
			return;
		}
		if (svcid == "MEMO_SAVE")
		{
			alert('데이터 저장 중 오류가 발생하였습니다.');
			fnMemoButton_Click(tbMain.tpMemo.btnMemoCancel);
			fnInitMemoGroup("INQUIRY");
			return;
		}
		if (svcid == "MEMO_DELETE")
		{
			alert('데이터 삭제 중 오류가 발생하였습니다.');
			fnInitMemoGroup("INQUIRY");
			return;
		}
	}
	
	if (svcid == "SAVE")
	{
		alert('저장되었습니다.');
		fnInquiry();
	}
	
	if (svcid == "SEARCH")
	{
		if (dsPT_M_SUSPENSION.rowcount ==0){
			dsPT_M_SUSPENSION.AddRow();
			dsPT_M_SUSPENSION.SetColumn(0, "WRT_NUM", dsPT_M_MASTER.GetColumn(0, "WRT_NUM"));
			dsPT_M_SUSPENSION.SetColumn(0, "TMP_WRT_NUM", dsPT_M_MASTER.GetColumn(0, "TMP_WRT_NUM"));
		}
		if (dsPT_M_NEGLIGENCE.rowcount ==0){
			dsPT_M_NEGLIGENCE.AddRow();
			dsPT_M_NEGLIGENCE.SetColumn(0, "WRT_NUM", dsPT_M_MASTER.GetColumn(0, "WRT_NUM"));
			dsPT_M_NEGLIGENCE.SetColumn(0, "TMP_WRT_NUM", dsPT_M_MASTER.GetColumn(0, "TMP_WRT_NUM"));
		}
		
		fnSusNegProc("Hide");
		
		Combo1_OnChanged();
	}
			
	if (svcid == "INSERT_SAVE")
	{	
		global.gv_WorkFlag = global.gv_MODIFY;
		fnInitButtonBar();
		
		if (dsPT_TEMPLET.rowcount > 0)
		{
			var User_ID = global.g_User_ID;
			var Form_ID = "CorrectMgrC";
			var sArgs = "User_ID=" + User_ID + " Form_ID=" + Form_ID;
			var sReturn = fnCreateDialog("st::TempletOutput.xml", sArgs);
		}
		
		dsPT_M_CORRECT.SetColumn(0, "WRT_NUM", dsPT_M_MASTER.GetColumn(0, "WRT_NUM"));
		dsPT_M_CORRECT.SetColumn(0, "TMP_WRT_NUM", dsPT_M_MASTER.GetColumn(0, "TMP_WRT_NUM"));				
	}
	
	if (svcid == "MEMO_SAVE")
	{
		alert('저장되었습니다.');
		fnInitMemoGroup("INQUIRY");
	}
	if (svcid == "MEMO_DELETE")
	{
		alert('삭제되었습니다.');
		fnInitMemoGroup("INQUIRY");
	}
	
	if (svcid == "NOTI_SAVE")
	{
		alert('저장되었습니다.');
		fnInitNotiGroup("INQUIRY");
	}
	if (svcid == "NOTI_DELETE")
	{
		alert('삭제되었습니다.');
		fnInitNotiGroup("INQUIRY");
	}
}

