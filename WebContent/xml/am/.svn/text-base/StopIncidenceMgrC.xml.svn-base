function fnKillFocus1(obj)
{
	if (obj == tbMain.tpProc.CORRECT_ORDER_END_DT)
	{
		if (!fnEmpty(tbMain.tpProc.CORRECT_ORDER_START_DT.Value) && !fnEmpty(tbMain.tpProc.CORRECT_ORDER_END_DT.Value))
		{
			var nEndDate = DateTime(tbMain.tpProc.CORRECT_ORDER_END_DT.Value);
			var nStdDate = DateTime(tbMain.tpProc.CORRECT_ORDER_START_DT.Value);
			
			if (nStdDate >= nEndDate)
			{
				alert('영업정지 종료일자가 시작일자보다 작거나 같습니다.');
				return;
			}
			
			tbMain.tpProc.SUSPENSION_DATE.Text = nEndDate - nStdDate;
		}
		return;
	}
}

