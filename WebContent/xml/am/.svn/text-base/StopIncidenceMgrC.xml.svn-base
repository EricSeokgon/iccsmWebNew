	var sCallBack = "fn_CallBack";
	
	Transaction(sTid, sUrl, sInDs, sOutDs, sArgs, sCallBack);
}

function fnSusNegProc(flag){

	if (flag =="Hide"){
	
			//예정처분 세팅
			dsPT_NEXT_DISPO.Filter("CODE='" + dsPT_M_MASTER.GetColumn(0, "DISPO_CONT")+"'");
			this.tbMain.tpProc.NEXT_DISPO.Index = 0;
					
		if (dsPT_M_MASTER.GetColumn(0, "DISPO_CONT") == "M00002" && dsPT_M_MASTER.GetColumn(0, "DISPO_CONT2") == ""){
			//영업정지 이면 과태료 비활성화
			fnControlState(false, this.tbMain.tpProc, "PAY_LIM_PERIOD,NEFI_MONEY");
			//업체통보관리 영업정지내용의  콤보박스
			this.tbMain.tpProc.GUBUN.InnerDataset = "dsGUBUN";this.tbMain.tpProc.GUBUN.column = "GUBUN";
			
		} else if (dsPT_M_MASTER.GetColumn(0, "DISPO_CONT") == "M00003" && dsPT_M_MASTER.GetColumn(0, "DISPO_CONT2") == ""){
			//과태료 이면 영업정지 비활성화		
			fnControlState(false, this.tbMain.tpProc, "BUSISUSP_START_DT,SUSPENSION_END_DATE,DISPO_PERIOD,OFFI_REPORT_DT,HOME_NOTE");
			//업체통보관리 과태료 내용의  콤보박스
			this.tbMain.tpProc.GUBUN.InnerDataset = "dsGUBUN_2";this.tbMain.tpProc.GUBUN.column = "GUBUN";
			//과태료 의견청취 내용을 과태료 TB로 변경
			this.tbMain.tpProc.OPINION_COLLECT_DATE.BindDataset = "dsPT_M_SUSPENSION";
			this.tbMain.tpProc.LIS_END_DATE.BindDataset = "dsPT_M_SUSPENSION";
			this.tbMain.tpProc.BEFORE_DISPO.BindDataset = "dsPT_M_SUSPENSION";
			this.tbMain.tpProc.OPINI_SUBMIT.BindDataset = "dsPT_M_SUSPENSION";
			this.tbMain.tpProc.OPINI_SUBMIT_DATE.BindDataset = "dsPT_M_SUSPENSION";
			this.tbMain.tpProc.OPINION_COLLECT_CONTENT.BindDataset = "dsPT_M_SUSPENSION";

			this.tbMain.tpProc.OPINION_COLLECT_DATE.Column = "DECISION_DATE";
			this.tbMain.tpProc.LIS_END_DATE.Column = "LIS_END_DATE";
			this.tbMain.tpProc.BEFORE_DISPO.Column = "BEFORE_DISPO";
			this.tbMain.tpProc.OPINI_SUBMIT.Column = "OPINI_SUBMIT";
			this.tbMain.tpProc.OPINI_SUBMIT_DATE.Column = "OPINI_SUBMIT_DATE";
			this.tbMain.tpProc.OPINION_COLLECT_CONTENT.Column = "OPIN_COLL_CONT";
			
		} else if (dsPT_M_MASTER.GetColumn(0, "DISPO_CONT") <> "" && dsPT_M_MASTER.GetColumn(0, "DISPO_CONT2") <> ""){
			//예정처분 세팅
			dsPT_NEXT_DISPO.Filter("CODE='M00000'");
			this.tbMain.tpProc.NEXT_DISPO.Index = 0;
			//업체통보관리 영업정지 및 과태료 내용의  콤보박스
			this.tbMain.tpProc.GUBUN.InnerDataset = "dsGUBUN_3";this.tbMain.tpProc.GUBUN.column = "GUBUN";
		}
		
	} else if (flag == "FromTo"){
	
		if (dsPT_M_MASTER.GetColumn(0, "DISPO_CONT") == "M00002" && dsPT_M_MASTER.GetColumn(0, "DISPO_CONT2") == ""){	
			// 영업정지시에는 과태료(의견청취) TB 초기화하여 저장 안함
			dsPT_M_NEGLIGENCE.DeleteAll();
		} else if (dsPT_M_MASTER.GetColumn(0, "DISPO_CONT") == "M00003" && dsPT_M_MASTER.GetColumn(0, "DISPO_CONT2") == ""){
			// 과태료부과시에는 화면 로딩시에 의견청취 TB를 과태료 TB로 변경하므로 이벤트 발생 안함
		} else if (dsPT_M_MASTER.GetColumn(0, "DISPO_CONT") == "M00002" && dsPT_M_MASTER.GetColumn(0, "DISPO_CONT2") == "M00003"){
			fnSustoNeg();
		} else if (dsPT_M_MASTER.GetColumn(0, "DISPO_CONT") == "M00003" && dsPT_M_MASTER.GetColumn(0, "DISPO_CONT2") == "M00002"){
			fnNegtoSus();
		}
	}
}

