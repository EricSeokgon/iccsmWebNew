			alert(sMessage);
			dsPT_M_MASTER.SetColumn(0, "MOT_STE", "C00001");
			return;
		}
	}
	
	//dsPT_M_SUSPENSION.SetColumn(dsPT_M_SUSPENSION.row,"OPIN_COLL_DT",dsPT_M_MASTER.GetColumn(0, "DISPO_DT"));

	fnSusNegProc("FromTo");
	
