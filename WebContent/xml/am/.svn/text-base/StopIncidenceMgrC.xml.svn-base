	dsPT_M_NEGLIGENCE.SetColumn(dsPT_M_NEGLIGENCE.row,"DESION_DATE",dsPT_M_SUSPENSION.GetColumn(dsPT_M_SUSPENSION.row,"DESION_DATE"));
	dsPT_M_NEGLIGENCE.SetColumn(dsPT_M_NEGLIGENCE.row,"LIS_END_DATE",dsPT_M_SUSPENSION.GetColumn(dsPT_M_SUSPENSION.row,"LIS_END_DATE"));	
	dsPT_M_NEGLIGENCE.SetColumn(dsPT_M_NEGLIGENCE.row,"BEFORE_DISPO",dsPT_M_SUSPENSION.GetColumn(dsPT_M_SUSPENSION.row,"BEFORE_DISPO"));		
	dsPT_M_NEGLIGENCE.SetColumn(dsPT_M_NEGLIGENCE.row,"OPINI_SUBMIT",dsPT_M_SUSPENSION.GetColumn(dsPT_M_SUSPENSION.row,"OPINI_SUBMIT"));		
	dsPT_M_NEGLIGENCE.SetColumn(dsPT_M_NEGLIGENCE.row,"OPINI_SUBMIT_DATE",dsPT_M_SUSPENSION.GetColumn(dsPT_M_SUSPENSION.row,"OPINI_SUBMIT_DATE"));			
	dsPT_M_NEGLIGENCE.SetColumn(dsPT_M_NEGLIGENCE.row,"OPIN_COLL_CONT","과태료부과");				
	dsPT_M_NEGLIGENCE.SetColumn(dsPT_M_NEGLIGENCE.row,"OPIN_COLL_DT",dsPT_M_MASTER.GetColumn(0, "DISPO_DT"));					
}

function fnNegtoSus(){//과태료 데이터를  영업정지  데이터로 복사
	dsPT_M_SUSPENSION.SetColumn(dsPT_M_SUSPENSION.row,"DESION_DATE",dsPT_M_NEGLIGENCE.GetColumn(dsPT_M_NEGLIGENCE.row,"DESION_DATE"));
	dsPT_M_SUSPENSION.SetColumn(dsPT_M_SUSPENSION.row,"LIS_END_DATE",dsPT_M_NEGLIGENCE.GetColumn(dsPT_M_NEGLIGENCE.row,"LIS_END_DATE"));	
	dsPT_M_SUSPENSION.SetColumn(dsPT_M_SUSPENSION.row,"BEFORE_DISPO",dsPT_M_NEGLIGENCE.GetColumn(dsPT_M_NEGLIGENCE.row,"BEFORE_DISPO"));		
	dsPT_M_SUSPENSION.SetColumn(dsPT_M_SUSPENSION.row,"OPINI_SUBMIT",dsPT_M_NEGLIGENCE.GetColumn(dsPT_M_NEGLIGENCE.row,"OPINI_SUBMIT"));		
	dsPT_M_SUSPENSION.SetColumn(dsPT_M_SUSPENSION.row,"OPINI_SUBMIT_DATE",dsPT_M_NEGLIGENCE.GetColumn(dsPT_M_NEGLIGENCE.row,"OPINI_SUBMIT_DATE"));			
	dsPT_M_SUSPENSION.SetColumn(dsPT_M_SUSPENSION.row,"OPIN_COLL_CONT","영업정지");				
	dsPT_M_SUSPENSION.SetColumn(dsPT_M_SUSPENSION.row,"OPIN_COLL_DT",dsPT_M_MASTER.GetColumn(0, "DISPO_DT"));					
}

