//////////////////////////////////////////////////////////////////
// Form Load 이벤트
//////////////////////////////////////////////////////////////////
function fnForm_Loaded(obj)
{
	global.gv_WorkFlag = _sWorkFlag;
	
	fnGetCodeValue();
	Combo1_OnChanged();
	
	if(global.gv_WorkFlag == global.gv_INQUIRY)
	{
		fnInquiry();
	}
	else if(global.gv_WorkFlag == global.gv_INSERT)
	{
		global.gv_WorkFlag = global.gv_INSERT;
		fnInitButtonBar();
	}
	else if(global.gv_WorkFlag == global.gv_MODIFY)
	{
		fnModify();
	}
	
	fnInitMemoGroup("INQUIRY");
	fnInitNotiGroup("INQUIRY");
	
	this.tbMain.tpProc.OPINION_COLLECT_DATE.Text = Today();
	this.tbMain.tpProc.Calendar0.Text = Today();
	this.tbMain.tpProc.Calendar1.Text = Today();
	this.tbMain.tpProc.Calendar3.Text = Today();
	this.tbMain.tpProc.SUSPENSION_START_DATE.Text = Today();
	this.tbMain.tpProc.SUSPENSION_END_DATE.Text = Today();
	this.tbMain.tpProc.Calendar2.Text = Today();
	this.tbMain.tpProc.Calendar4.Text =Today();
	this.tbMain.tpProc.Calendar5.Text =Today();
	this.tbMain.tpProc.Calendar6.Text = Today();
	
	
	divButtonBar.Image1.text = space(6) + "영업정지";
}

function fnForm_Activate(obj)
{
